<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.pennant</groupId>
	<artifactId>PFSBatch</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>PFSBatch</name>
	<url>http://www.pennanttech.com</url>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<jdk.version>1.6</jdk.version>
	</properties>

	<repositories>
		<repository>
			<id>central</id>
			<name>Central Repository</name>
			<url>http://192.168.1.10:8080/nexus/content/groups/public/</url>
		</repository>
	</repositories>

	<dependencies>
		<dependency>
			<groupId>com.pennant</groupId>
			<artifactId>PFSJava</artifactId>
			<version>0.0.1-SNAPSHOT</version>
			<exclusions>
			
				<!-- Spring Security -->
				<exclusion>
					<groupId>org.springframework.security</groupId>
					<artifactId>spring-security-taglibs</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.springframework.security</groupId>
					<artifactId>spring-security-config</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.springframework.security</groupId>
					<artifactId>spring-security-ldap</artifactId>
				</exclusion>
				
				<!-- Zkoss Framework -->
				
				<exclusion>
					<groupId>org.zkoss.zk</groupId>
					<artifactId>zkplus</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.zk</groupId>
					<artifactId>zkmax</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.zk</groupId>
					<artifactId>zkex</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.zk</groupId>
					<artifactId>zkbind</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.zk</groupId>
					<artifactId>zhtml</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss</groupId>
					<artifactId>codemirror</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.zkforge</groupId>
					<artifactId>ckez</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.zkforge.el</groupId>
					<artifactId>zcommons-el</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.calendar</groupId>
					<artifactId>calendar</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.exporter</groupId>
					<artifactId>exporter</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.zkoss.fusionchart</groupId>
					<artifactId>fusionchart</artifactId>
				</exclusion>
				
				<!-- Jasper Reports -->
				<exclusion>
					<groupId>net.sf.jasperreports</groupId>
					<artifactId>jasperreports</artifactId>
				</exclusion>
				<exclusion>
					<groupId>net.sf.jasperreports</groupId>
					<artifactId>jasperreports-chart-themes</artifactId>
				</exclusion>
				<exclusion>
					<groupId>com.pennant.jasper</groupId>
					<artifactId>TrebuchetMS</artifactId>
				</exclusion>
				<exclusion>
					<groupId>com.pennant.jasper</groupId>
					<artifactId>w3c</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.w3c.css</groupId>
					<artifactId>sac</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.apache.xmlgraphics</groupId>
					<artifactId>batik-svggen</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.apache.xmlgraphics</groupId>
					<artifactId>batik-bridge</artifactId>
				</exclusion>
				
				<!-- Mail -->
				<exclusion>
					<groupId>javax.mail</groupId>
					<artifactId>mail</artifactId>
				</exclusion>
				
				<!-- Commons -->
				<exclusion>
					<groupId>org.jasypt</groupId>
					<artifactId>jasypt-spring3</artifactId>
				</exclusion>
				<exclusion>
					<groupId>commons-dbcp</groupId>
					<artifactId>commons-dbcp</artifactId>
				</exclusion>
				<exclusion>
					<groupId>commons-io</groupId>
					<artifactId>commons-io</artifactId>
				</exclusion>
				<exclusion>
					<groupId>commons-collections</groupId>
					<artifactId>commons-collections</artifactId>
				</exclusion>
				<exclusion>
					<groupId>javax.servlet</groupId>
					<artifactId>servlet-api</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.freemarker</groupId>
					<artifactId>freemarker</artifactId>
				</exclusion>
				<exclusion>
					<groupId>com.healthmarketscience.sqlbuilder</groupId>
					<artifactId>sqlbuilder</artifactId>
				</exclusion>
				<exclusion>
					<groupId>net.objectlab.kit</groupId>
					<artifactId>datecalc-common</artifactId>
				</exclusion>
				<exclusion>
					<groupId>net.objectlab.kit</groupId>
					<artifactId>datecalc-jdk</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.codehaus.groovy</groupId>
					<artifactId>groovy</artifactId>
				</exclusion>
				
				<!-- Axiom Jars -->
				<exclusion>
					<groupId>org.apache.ws.commons.axiom</groupId>
					<artifactId>axiom-api</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.apache.ws.commons.axiom</groupId>
					<artifactId>axiom-impl</artifactId>
				</exclusion>
				
				<!-- External Jars -->
				<exclusion>
					<groupId>com.pennant.aspose</groupId>
					<artifactId>Aspose.Words.jdk16</artifactId>
				</exclusion>
				<exclusion>
					<groupId>com.pennant.document.generator</groupId>
					<artifactId>pas-dte</artifactId>
				</exclusion>
				
			</exclusions>
		</dependency>
	</dependencies>

	<build>
		<sourceDirectory>src</sourceDirectory>
		<resources>
			<resource>
				<directory>src</directory>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>${jdk.version}</source>
					<target>${jdk.version}</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>