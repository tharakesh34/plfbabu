<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="window_SystemInternalAccountDefinitionList"
		apply="${systemInternalAccountDefinitionListCtrl}" border="none"
		width="100%">

		<div sclass="z-toolbar" style="padding:0px" height="28px">
			<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
				width="100%" widths="30%,40%,30%">

				<!-- COMMON BUTTONS -->
				<label value="" />

				<!-- SEARCH AREA -->
				<toolbar align="center" sclass="toolbar-center">
					<button
						id="button_SystemInternalAccountDefinitionList_SystemInternalAccountDefinitionSearchDialog"
						 label="${c:l('label_Filter')}"
						autodisable="button_SystemInternalAccountDefinitionList_SystemInternalAccountDefinitionSearchDialog,button_SystemInternalAccountDefinitionList_PrintList,button_SystemInternalAccountDefinitionList_NewSystemInternalAccountDefinition,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_SystemInternalAccountDefinitionList_SystemInternalAccountDefinitionSearchDialog.tooltiptext')}" />
					<button
						id="button_SystemInternalAccountDefinitionList_PrintList"
						 label="${c:l('label_Print')}"
						autodisable="button_SystemInternalAccountDefinitionList_SystemInternalAccountDefinitionSearchDialog,button_SystemInternalAccountDefinitionList_PrintList,button_SystemInternalAccountDefinitionList_NewSystemInternalAccountDefinition,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_SystemInternalAccountDefinitionList_PrintList.tooltiptext')}" />
				</toolbar>

				<!-- CRUD BUTTONS -->
				<toolbar align="end" sclass="toolbar-end">
					<button
						id="button_SystemInternalAccountDefinitionList_NewSystemInternalAccountDefinition"
						 label="${c:l('label_New')}"
						autodisable="button_SystemInternalAccountDefinitionList_SystemInternalAccountDefinitionSearchDialog,button_SystemInternalAccountDefinitionList_PrintList,button_SystemInternalAccountDefinitionList_NewSystemInternalAccountDefinition,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_SystemInternalAccountDefinitionList_NewSystemInternalAccountDefinition.tooltiptext')}" />
					<button id="btnRefresh" 
						label="${c:l('label_Refresh')}"
						autodisable="button_SystemInternalAccountDefinitionList_SystemInternalAccountDefinitionSearchDialog,button_SystemInternalAccountDefinitionList_PrintList,button_SystemInternalAccountDefinitionList_NewSystemInternalAccountDefinition,btnRefresh,btnHelp"
						tooltiptext="${c:l('btnRefresh.tooltiptext')}" />
					<button id="btnHelp" 
						label="${c:l('label_Help')}"
						autodisable="button_SystemInternalAccountDefinitionList_SystemInternalAccountDefinitionSearchDialog,button_SystemInternalAccountDefinitionList_PrintList,button_SystemInternalAccountDefinitionList_NewSystemInternalAccountDefinition,btnRefresh,btnHelp"
						tooltiptext="${c:l('btnHelp.tooltiptext')}" />
				</toolbar>
			</hbox>
		</div>

		<borderlayout
			id="borderLayout_SystemInternalAccountDefinitionList">
			<north border="none" />

			<center border="none">
				<div>
					<grid id="searchGrid" sclass="GridLayoutNoBorder"
						fixedLayout="true" style="border:0px">
						<columns>
							<column width="17%" />
							<column width="5%" />
							<column width="28%" />
							<column width="17%" />
							<column width="5%" />
							<column width="28%" />
						</columns>
						<rows>
							<row>
								<label
									id="label_SystemInternalAccountDefinitionSearch_SIACode"
									value="${c:l('label_SystemInternalAccountDefinitionSearch_SIACode.value')}" />
								<listbox id="sortOperator_sIACode"
									width="43px" mold="select" rows="1" />
								<uppercasebox id="sIACode" width="72px"
									maxlength="8" />
								<label
									id="label_SystemInternalAccountDefinitionSearch_SIAName"
									value="${c:l('label_SystemInternalAccountDefinitionSearch_SIAName.value')}" />
								<listbox id="sortOperator_sIAName"
									width="43px" mold="select" rows="1" />
								<textbox id="sIAName" width="150px"
									maxlength="50" />
							</row>
							<row>
								<label
									id="label_SystemInternalAccountDefinitionSearch_SIAShortName"
									value="${c:l('label_SystemInternalAccountDefinitionSearch_SIAShortName.value')}" />
								<listbox id="sortOperator_sIAShortName"
									width="43px" mold="select" rows="1" />
								<textbox id="sIAShortName" width="135px"
									maxlength="15" />
								<label
									id="label_SystemInternalAccountDefinitionSearch_SIAAcType"
									value="${c:l('label_SystemInternalAccountDefinitionSearch_SIAAcType.value')}" />
								<listbox id="sortOperator_sIAAcType"
									width="43px" mold="select" rows="1" />
								<textbox id="sIAAcType" width="72px"
									maxlength="8" />
							</row>
							<row>
								<label
									id="label_SystemInternalAccountDefinitionSearch_SIANumber"
									value="${c:l('label_SystemInternalAccountDefinitionSearch_SIANumber.value')}" />
								<listbox id="sortOperator_sIANumber"
									width="43px" mold="select" rows="1" />
								<textbox id="sIANumber" width="72px"
									maxlength="8" />
							</row>
							<row>
								<label
									id="label_SystemInternalAccountDefinitionSearch_RecordStatus"
									value="${c:l('label_SystemInternalAccountDefinitionSearch_RecordStatus.value')}" />
								<listbox id="sortOperator_recordStatus"
									width="43px" mold="select" rows="1" />
								<textbox id="recordStatus" width="100px"
									maxlength="50" />
								<label
									id="label_SystemInternalAccountDefinitionSearch_RecordType"
									value="${c:l('label_SystemInternalAccountDefinitionSearch_RecordType.value')}" />
								<listbox id="sortOperator_recordType"
									width="43px" mold="select" rows="1" />
								<listbox id="recordType" width="100px"
									mold="select" />
							</row>
						</rows>
					</grid>
					<listbox id="listBoxSystemInternalAccountDefinition"
						fixedLayout="true" vflex="true" span="true"
						tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
						height="100%" multiple="false">
						<listhead sizable="true">
							<listheader id="listheader_SIACode"
								label="${c:l('listheader_SIACode.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_SIAName"
								label="${c:l('listheader_SIAName.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_SIAShortName"
								label="${c:l('listheader_SIAShortName.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_SIAAcType"
								label="${c:l('listheader_SIAAcType.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_SIANumber"
								label="${c:l('listheader_SIANumber.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_RecordStatus"
								label="${c:l('listheader_RecordStatus.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_RecordType"
								label="${c:l('listheader_RecordType.label')}" sort="auto"
								hflex="min" />
						</listhead>
					</listbox>
				</div>
			</center>
			<south border="none">
				<paging id="pagingSystemInternalAccountDefinitionList" />
			</south>

		</borderlayout>
	</window>
</zk>