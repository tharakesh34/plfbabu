<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">
	<window>
		<window id="window_ExtFieldConfigList"  apply="${extFieldConfigListCtrl}" border="none" width="100%" >
			<div sclass="z-toolbar" style="padding:0px">
				<hbox pack="stretch" sclass="hboxRemoveWhiteStrips" width="100%" widths="35%,30%,35%">
	
					<!-- COMMON BUTTONS -->
					<label value="" />	
					<!-- SEARCH AREA -->
				<toolbar align="center"  sclass="toolbar-center">
					<button
						id="button_ExtendedFieldHeaderList_ExtendedFieldHeaderSearchDialog"
						label="${c:l('label_Filter')}"
						autodisable="button_ExtendedFieldHeaderList_ExtendedFieldHeaderSearchDialog,print,button_ExtendedFieldHeaderList_NewExtendedFieldHeader,btnRefresh,help"
						tooltiptext="${c:l('button_ExtendedFieldHeaderList_ExtendedFieldHeaderSearchDialog.tooltiptext')}" sclass="z-toolbarbutton" />
					<button
						id="print"
						label="${c:l('label_Print')}"
						autodisable="button_ExtendedFieldHeaderList_ExtendedFieldHeaderSearchDialog,print,button_ExtendedFieldHeaderList_NewExtendedFieldHeader,btnRefresh,help"
						tooltiptext="${c:l('button_ExtendedFieldHeaderList_PrintList.tooltiptext')}" sclass="z-toolbarbutton" />
				</toolbar>

				<!-- CRUD BUTTONS -->
				<toolbar align="end"  sclass="toolbar-end">
					<button
						id="button_ExtendedFieldHeaderList_NewExtendedFieldHeader"
						label="${c:l('label_New')}"
						autodisable="button_ExtendedFieldHeaderList_ExtendedFieldHeaderSearchDialog,print,button_ExtendedFieldHeaderList_NewExtendedFieldHeader,btnRefresh,help"
						tooltiptext="${c:l('button_ExtendedFieldHeaderList_NewExtendedFieldHeader.tooltiptext')}" sclass="z-toolbarbutton" />
					<button id="btnRefresh"
						label="${c:l('label_Refresh')}"
						autodisable="button_ExtendedFieldHeaderList_ExtendedFieldHeaderSearchDialog,print,button_ExtendedFieldHeaderList_NewExtendedFieldHeader,btnRefresh,help"
						tooltiptext="${c:l('btnRefresh.tooltiptext')}" sclass="z-toolbarbutton" />
					<button id="help" visible="false" label="${c:l('label_Help')}"
						autodisable="button_ExtendedFieldHeaderList_ExtendedFieldHeaderSearchDialog,print,button_ExtendedFieldHeaderList_NewExtendedFieldHeader,btnRefresh,help"
						tooltiptext="${c:l('btnHelp.tooltiptext')}" sclass="z-toolbarbutton" />
				</toolbar>
				</hbox>
			</div>
			<borderlayout id="borderLayout_ExtFieldConfigList">
				<north border="none" />
				<center border="none">
					<div>
						<grid id="searchGrid" sclass="GridLayoutNoBorder" fixedLayout="true" >
							<columns>
								<column width="180px" />
								<column width="50px" />
								<column />
								<column width="180px" />
								<column width="50px" />
								<column />
							</columns>
							<rows>
								<row>
									<label value="${c:l('label_ExtendedFieldConfig_Module.value')}" />
									<listbox id="sortOperator_ModuleName" width="43px" mold="select" rows="1" emptyMessage="${c:l('listbox.NoEmptyMessage')}" />
									<uppercasebox id="moduleName" width="110px" maxlength="8" />
									
									<label value="${c:l('label_ExtendedFieldConfig_SubModule.value')}" />
									<listbox id="sortOperator_SubModuleName" width="43px" mold="select" rows="1" emptyMessage="${c:l('listbox.NoEmptyMessage')}" />
									<textbox id="subModuleName" maxlength="50" width="200px"/>
									
								</row>
								<row>
									<label  value="${c:l('label_ExtendedFieldConfig_PreValidationReq.value')}" />
									<listbox id="sortOperator_PreValidationReq" width="43px" mold="select" rows="1" emptyMessage="${c:l('listbox.NoEmptyMessage')}" />
									<checkbox id="preValidationReq" checked="false"/>
									
									<label  value="${c:l('label_ExtendedFieldConfig_PostValidationReq.value')}" />
									<listbox id="sortOperator_PostValidationReq" width="43px" mold="select" rows="1" emptyMessage="${c:l('listbox.NoEmptyMessage')}" />
									<checkbox id="postValidationReq" checked="false"/>
								</row>
								<row>
									<label  value="${c:l('label_ExtendedFieldConfig_TabHeading.value')}" />
									<listbox id="sortOperator_TabHeading" width="43px" mold="select" rows="1" emptyMessage="${c:l('listbox.NoEmptyMessage')}" />
									<textbox id="tabHeading"/>
								</row>
								<row>
									<label value="${c:l('label_ExtendedFieldConfig_RecordStatus.value')}" />
									<listbox id="sortOperator_RecordStatus" width="43px" mold="select" rows="1" />
									<textbox id="recordStatus" width="100px" maxlength="50" />
									
									<label  value="${c:l('label_ExtendedFieldConfig_RecordOperation.value')}" />
									<listbox id="sortOperator_RecordType" width="43px" mold="select" rows="1" />
									<listbox id="recordType" width="100px" mold="select" />
								</row>
							</rows>
						</grid>

					<listbox id="listBoxExtFieldConfig" emptyMessage="${c:l('listbox.emptyMessage')}" tooltiptext="${c:l('listbox.tooltiptext')}" width="100%" height="100%" multiple="false" sizedByContent="true" span="true">
						<listhead sizable="true">
							<listheader id="listheader_ModuleName" label="${c:l('label_ExtendedFieldConfig_Module.value')}" sort="auto" hflex="min"  />
							<listheader id="listheader_SubModuleName" label="${c:l('label_ExtendedFieldConfig_SubModule.value')}" sort="auto" hflex="min"  />
							<listheader id="listheader_TabHeading" label="${c:l('label_ExtendedFieldConfig_TabHeading.value')}" sort="auto"  hflex="min"  />
							<listheader id="listheader_PreValidationReq" label="${c:l('label_ExtendedFieldConfig_PreValidationReq.value')}" sort="auto"  hflex="min"  />
							<listheader id="listheader_PostValidationReq" label="${c:l('label_ExtendedFieldConfig_PostValidationReq.value')}" sort="auto"  hflex="min"  />
							<listheader id="listheader_RecordStatus" label="${c:l('label.RecordStatus')}" sort="auto" hflex="min"  />
							<listheader id="listheader_RecordType"  label="${c:l('label.RecordType')}" sort="auto" hflex="min"  />
						</listhead>		
					</listbox>
				</div>	
				</center>

				<south border="none" >
					<paging id="pagingExtFieldConfigList" height="35px"/>
				</south>
			</borderlayout>
	</window>
</window>
</zk>