<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="window_ExtendedFieldDetailList"
		apply="${extendedFieldDetailListCtrl}" border="none" width="100%">
		
		<div sclass="z-toolbar" style="padding:0px" height="28px">
			<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
				width="100%" widths="30%,40%,30%">

				<!-- COMMON BUTTONS -->
				<label value="" />

				<!-- SEARCH AREA -->
				<toolbar align="center" style="border-style: none;">
					<button
						id="button_ExtendedFieldDetailList_ExtendedFieldDetailSearchDialog"
						 label = "${c:l('label_Filter')}"
						autodisable="button_ExtendedFieldDetailList_ExtendedFieldDetailSearchDialog,button_ExtendedFieldDetailList_PrintList,button_ExtendedFieldDetailList_NewExtendedFieldDetail,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_ExtendedFieldDetailList_ExtendedFieldDetailSearchDialog.tooltiptext')}" />
					<button
						id="button_ExtendedFieldDetailList_PrintList" 
						label = "${c:l('label_Print')}"
						autodisable="button_ExtendedFieldDetailList_ExtendedFieldDetailSearchDialog,button_ExtendedFieldDetailList_PrintList,button_ExtendedFieldDetailList_NewExtendedFieldDetail,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_ExtendedFieldDetailList_PrintList.tooltiptext')}" />
				</toolbar>

				<!-- CRUD BUTTONS -->
				<toolbar align="end" style="border-style: none;">
					<!-- <button id="button_ExtendedFieldDetailList_NewExtendedFieldDetail"
						 label = "${c:l('label_New')}"
						autodisable="button_ExtendedFieldDetailList_ExtendedFieldDetailSearchDialog,button_ExtendedFieldDetailList_PrintList,button_ExtendedFieldDetailList_NewExtendedFieldDetail,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_ExtendedFieldDetailList_NewExtendedFieldDetail.tooltiptext')}" /> -->
					<button id="btnRefresh" 
						label = "${c:l('label_Refresh')}"
						autodisable="button_ExtendedFieldDetailList_ExtendedFieldDetailSearchDialog,button_ExtendedFieldDetailList_PrintList,button_ExtendedFieldDetailList_NewExtendedFieldDetail,btnRefresh,btnHelp"
						tooltiptext="${c:l('btnRefresh.tooltiptext')}" />
					<button id="btnHelp" 
						label = "${c:l('label_Help')}"
						autodisable="button_ExtendedFieldDetailList_ExtendedFieldDetailSearchDialog,button_ExtendedFieldDetailList_PrintList,button_ExtendedFieldDetailList_NewExtendedFieldDetail,btnRefresh,btnHelp"
						tooltiptext="${c:l('btnHelp.tooltiptext')}" />
				</toolbar>
			</hbox>
		</div>

		<borderlayout id="borderLayout_ExtendedFieldDetailList">
			<north border="none" />

			<center border="none">
<div>
					<grid id="searchGrid" sclass="GridLayoutNoBorder"
						fixedLayout="true" style="border:0px">
						<columns>
							<column width="17%" />
							<column width="5%" />
							<column width="28%" />
							<column width="17%" />
							<column width="5%" />
							<column width="28%" />
						</columns>
						<rows>
							<row>
								<label
									id="label_ExtendedFieldDetailSearch_ModuleName"
									value="${c:l('label_ExtendedFieldDetailSearch_ModuleName.value')}" />
								<listbox id="sortOperator_moduleName"
									width="43px" mold="select" rows="1" />
								<textbox id="moduleName" width="250px"
									maxlength="50" />
								<label
									id="label_ExtendedFieldDetailSearch_SubModuleName"
									value="${c:l('label_ExtendedFieldDetailSearch_SubModuleName.value')}" />
								<listbox id="sortOperator_subModuleName"
									width="43px" mold="select" rows="1" />
								<textbox id="subModuleName"
									width="250px" maxlength="50" />
							</row>
							<row>
								<label id="label_ExtendedFieldDetailSearch_RecordStatus" value="${c:l('label_ExtendedFieldDetailSearch_RecordStatus.value')}" />
									<listbox id="sortOperator_recordStatus" width="43px" mold="select" rows="1" />
										<textbox id="recordStatus" width="100px" maxlength="50" />
								<label id="label_ExtendedFieldDetailSearch_RecordType" value="${c:l('label_ExtendedFieldDetailSearch_RecordType.value')}" />
									<listbox id="sortOperator_recordType" width="43px" mold="select" rows="1" />
										<listbox id="recordType" width="100px" mold="select" />
							</row>
						</rows>
					</grid>
				<listbox id="listBoxExtendedFieldDetail" fixedLayout="true" vflex="true" span="true"
					tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
					height="100%" multiple="false">
					<listhead sizable="true">
						<listheader id="listheader_FieldName"
							label="${c:l('listheader_ModuleName.label')}" sort="auto"
							hflex="min" />
						<listheader id="listheader_FieldType"
							label="${c:l('listheader_SubModuleName.label')}" sort="auto"
							hflex="min" />
						<listheader id="listheader_RecordStatus"
							label="${c:l('listheader_RecordStatus.label')}" sort="auto"
							hflex="min" />
						<listheader id="listheader_RecordType"
							label="${c:l('listheader_RecordType.label')}" sort="auto"
							hflex="min" />
					</listhead>
				</listbox>
				</div>
			</center>
			<south border="none">
				<paging id="pagingExtendedFieldDetailList"
					  />
			</south>

		</borderlayout>
	</window>
</zk>