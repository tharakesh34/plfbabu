<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window>
		<window id="window_CorpRelationCodeList"
			apply="${corpRelationCodeListCtrl}" border="none" width="100%">

			<div sclass="z-toolbar" style="padding:0px" height="28px">
				<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
					width="100%" widths="30%,40%,30%">

					<!-- COMMON BUTTONS -->
					<label value="" />

					<!-- SEARCH AREA -->
					<toolbar align="center" sclass="toolbar-center">
						<button
							id="button_CorpRelationCodeList_CorpRelationCodeSearchDialog"
							 label = "${c:l('label_Filter')}"
							autodisable="button_CorpRelationCodeList_CorpRelationCodeSearchDialog,button_CorpRelationCodeList_PrintList,button_CorpRelationCodeList_NewCorpRelationCode, btnRefresh, btnHelp"
							tooltiptext="${c:l('button_CorpRelationCodeList_CorpRelationCodeSearchDialog.tooltiptext')}" sclass="z-toolbarbutton" />
						<button
							id="button_CorpRelationCodeList_PrintList" 
							label = "${c:l('label_Print')}"
							autodisable="button_CorpRelationCodeList_CorpRelationCodeSearchDialog,button_CorpRelationCodeList_PrintList,button_CorpRelationCodeList_NewCorpRelationCode, btnRefresh, btnHelp"
							tooltiptext="${c:l('button_CorpRelationCodeList_PrintList.tooltiptext')}" sclass="z-toolbarbutton" />
					</toolbar>

					<!-- CRUD BUTTONS -->
					<toolbar align="end"  sclass="toolbar-end">
						<button
							id="button_CorpRelationCodeList_NewCorpRelationCode"
							 label = "${c:l('label_New')}"
							autodisable="button_CorpRelationCodeList_CorpRelationCodeSearchDialog,button_CorpRelationCodeList_PrintList,button_CorpRelationCodeList_NewCorpRelationCode, btnRefresh, btnHelp"
							tooltiptext="${c:l('button_CorpRelationCodeList_NewCorpRelationCode.tooltiptext')}" sclass="z-toolbarbutton" />
						<button id="btnRefresh" 
							label = "${c:l('label_Refresh')}"
							autodisable="button_CorpRelationCodeList_CorpRelationCodeSearchDialog,button_CorpRelationCodeList_PrintList,button_CorpRelationCodeList_NewCorpRelationCode, btnRefresh, btnHelp"
							tooltiptext="${c:l('btnRefresh.tooltiptext')}" sclass="z-toolbarbutton" />
						<button id="btnHelp" 
							label = "${c:l('label_Help')}"
							autodisable="button_CorpRelationCodeList_CorpRelationCodeSearchDialog,button_CorpRelationCodeList_PrintList,button_CorpRelationCodeList_NewCorpRelationCode, btnRefresh, btnHelp"
							tooltiptext="${c:l('btnHelp.tooltiptext')}" sclass="z-toolbarbutton" />
					</toolbar>
				</hbox>
			</div>

			<borderlayout id="borderLayout_CorpRelationCodeList">
				<north border="none" />
				<center border="none">
					<div>
						<grid id="searchGrid"
							sclass="GridLayoutNoBorder" fixedLayout="true"
							style="border:0px; padding-left:5px; padding-right:5px;">
							<columns>
								<column width="130px" />
								<column width="50px" />
								<column />
								<column width="130px" />
								<column width="50px" />
								<column />
							</columns>
							<rows>
								<row>
									<label
										id="label_CorpRelationCodeSearch_CorpRelationCode"
										value="${c:l('label_CorpRelationCodeSearch_CorpRelationCode.value')}" />
									<listbox
										id="sortOperator_corpRelationCode" width="43px" mold="select"
										rows="1" />
									<uppercasebox id="corpRelationCode"
										width="100x" maxlength="8" />
									<label
										id="label_CorpRelationCodeSearch_CorpRelationDesc"
										value="${c:l('label_CorpRelationCodeSearch_CorpRelationDesc.value')}" />
									<listbox
										id="sortOperator_corpRelationDesc" width="43px" mold="select"
										rows="1" />
									<textbox id="corpRelationDesc"
										width="300px" maxlength="50" />
								</row>
								<row>
									<label
										id="label_CorpRelationCodeSearch_CorpRelationIsActive"
										value="${c:l('label_CorpRelationCodeSearch_CorpRelationIsActive.value')}" />
									<listbox
										id="sortOperator_corpRelationIsActive" width="43px"
										mold="select" rows="1" />
									<checkbox id="corpRelationIsActive"
										checked="false" />
								</row>
								<row id="row_AlwWorkflow">
									<label
										id="label_CorpRelationCodeSearch_RecordStatus"
										value="${c:l('label_CorpRelationCodeSearch_RecordStatus.value')}" />
									<listbox
										id="sortOperator_recordStatus" width="43px" mold="select"
										rows="1" />
									<textbox id="recordStatus"
										width="100px" maxlength="50" />
									<label
										id="label_CorpRelationCodeSearch_RecordType"
										value="${c:l('label_CorpRelationCodeSearch_RecordType.value')}" />
									<listbox
										id="sortOperator_recordType" width="43px" mold="select"
										rows="1" />
									<listbox id="recordType"
										width="100px" mold="select" />
								</row>
							</rows>
						</grid>
						<listbox id="listBoxCorpRelationCode"
							vflex="true" tooltiptext="${c:l('listbox.tooltiptext')}"
							width="100%" height="100%" multiple="false" span="true">
							<listhead sizable="true">
								<listheader
									id="listheader_CorpRelationCode"
									label="${c:l('listheader_CorpRelationCode.label')}" sort="auto"
									hflex="min" />
								<listheader
									id="listheader_CorpRelationDesc"
									label="${c:l('listheader_CorpRelationDesc.label')}" sort="auto"
									hflex="min" />
								<listheader
									id="listheader_CorpRelationIsActive"
									label="${c:l('listheader_CorpRelationIsActive.label')}"
									sort="auto" hflex="min" />
								<listheader id="listheader_RecordStatus"
									label="${c:l('listheader_RecordStatus.label')}" sort="auto"
									hflex="min" />
								<listheader id="listheader_RecordType"
									label="${c:l('listheader_RecordType.label')}" sort="auto"
									hflex="min" />
							</listhead>
						</listbox>
					</div>
				</center>
				<south border="none">
					<paging id="pagingCorpRelationCodeList" />
				</south>

			</borderlayout>
		</window>
	</window>
</zk>