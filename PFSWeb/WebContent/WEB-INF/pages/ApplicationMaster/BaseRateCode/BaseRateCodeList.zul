<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="window_BaseRateCodeList" apply="${baseRateCodeListCtrl}"
		border="none" width="100%">

		<div sclass="z-toolbar" style="padding:0" height="28px">
			<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
				width="100%" widths="30%,40%,30%">

				<!-- COMMON BUTTONS -->
				<label value="" />

				<!-- SEARCH AREA -->
				<toolbar align="center" style="border-style: none;">
					<button
						id="button_BaseRateCodeList_BaseRateCodeSearchDialog"
						 label = "${c:l('label_Filter')}"
						autodisable="button_BaseRateCodeList_BaseRateCodeSearchDialog,button_BaseRateCodeList_PrintList,button_BaseRateCodeList_NewBaseRateCode,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_BaseRateCodeList_BaseRateCodeSearchDialog.tooltiptext')}" />
					<button id="button_BaseRateCodeList_PrintList"
						 label = "${c:l('label_Print')}"
						autodisable="button_BaseRateCodeList_BaseRateCodeSearchDialog,button_BaseRateCodeList_PrintList,button_BaseRateCodeList_NewBaseRateCode,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_BaseRateCodeList_PrintList.tooltiptext')}" />
				</toolbar>

				<!-- CRUD BUTTONS -->
				<toolbar align="end" style="border-style: none;">
					<button id="button_BaseRateCodeList_NewBaseRateCode"
						 label = "${c:l('label_New')}"
						autodisable="button_BaseRateCodeList_BaseRateCodeSearchDialog,button_BaseRateCodeList_PrintList,button_BaseRateCodeList_NewBaseRateCode,btnRefresh,btnHelp"
						tooltiptext="${c:l('button_BaseRateCodeList_NewBaseRateCode.tooltiptext')}" />
					<button id="btnRefresh" 
						label = "${c:l('label_Refresh')}"
						autodisable="button_BaseRateCodeList_BaseRateCodeSearchDialog,button_BaseRateCodeList_PrintList,button_BaseRateCodeList_NewBaseRateCode,btnRefresh,btnHelp"
						tooltiptext="${c:l('btnRefresh.tooltiptext')}" />
					<button id="btnHelp" 
						label = "${c:l('label_Help')}"
						autodisable="button_BaseRateCodeList_BaseRateCodeSearchDialog,button_BaseRateCodeList_PrintList,button_BaseRateCodeList_NewBaseRateCode,btnRefresh,btnHelp"
						tooltiptext="${c:l('btnHelp.tooltiptext')}" />
				</toolbar>
			</hbox>
		</div>

		<borderlayout id="borderLayout_BaseRateCodeList">
			<north border="none" />
			<center border="none">
				<div>
					<grid id="searchGrid" sclass="GridLayoutNoBorder"
						fixedLayout="true"
						style="border:0px; padding-left:5px; padding-right:5px;">
						<columns>
							<column width="130px" />
							<column width="50px" />
							<column />
							<column width="130px" />
							<column width="50px" />
							<column />
						</columns>
						<rows>
							<row>
								<label
									id="label_BaseRateCodeSearch_BRType"
									value="${c:l('label_BaseRateCodeSearch_BRType.value')}" />
								<listbox id="sortOperator_bRType"
									width="43px" mold="select" rows="1" />
								<uppercasebox id="bRType" width="96px"
									maxlength="8" />
								<label
									id="label_BaseRateCodeSearch_BRTypeDesc"
									value="${c:l('label_BaseRateCodeSearch_BRTypeDesc.value')}" />
								<listbox id="sortOperator_bRTypeDesc"
									width="43px" mold="select" rows="1" />
								<textbox id="bRTypeDesc" width="300px"
									maxlength="50" />
							</row>
							<row id="row_AlwWorkflow">
								<label
									id="label_BaseRateCodeSearch_RecordStatus"
									value="${c:l('label_BaseRateCodeSearch_RecordStatus.value')}" />
								<listbox id="sortOperator_recordStatus"
									width="43px" mold="select" rows="1" />
								<textbox id="recordStatus" width="100px"
									maxlength="50" />
								<label
									id="label_BaseRateCodeSearch_RecordType"
									value="${c:l('label_BaseRateCodeSearch_RecordType.value')}" />
								<listbox id="sortOperator_recordType"
									width="43px" mold="select" rows="1" />
								<listbox id="recordType" width="100px"
									mold="select" />
							</row>
						</rows>
					</grid>
					<listbox id="listBoxBaseRateCode" vflex="true"
						tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
						height="100%" multiple="false" span="true">
						<listhead sizable="true">
							<listheader id="listheader_BRType"
								label="${c:l('listheader_BRType.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_BRTypeDesc"
								label="${c:l('listheader_BRTypeDesc.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_RecordStatus"
								label="${c:l('listheader_RecordStatus.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_RecordType"
								label="${c:l('listheader_RecordType.label')}" sort="auto"
								hflex="min" />
						</listhead>
					</listbox>
				</div>
			</center>
			<south border="none">
				<paging id="pagingBaseRateCodeList" />
			</south>

		</borderlayout>
	</window>
</zk>