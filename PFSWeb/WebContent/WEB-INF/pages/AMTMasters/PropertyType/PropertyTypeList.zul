<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="window_PropertyTypeList"  apply="${propertyTypeListCtrl}"
		border="none" width="100%">
			<div sclass="z-toolbar" style="padding:0px" height="28px">
				<hbox pack="stretch" sclass="hboxRemoveWhiteStrips" width="100%" widths="30%,40%,30%">
	
					<!-- COMMON BUTTONS -->
					<label value="" />	
					<!-- SEARCH AREA -->
					<toolbar align="center"  sclass="toolbar-center">
						<button
							id="button_PropertyTypeList_PropertyTypeSearchDialog" 
							label = "${c:l('label_Filter')}"
							autodisable="button_PropertyTypeList_PropertyTypeSearchDialog,button_PropertyTypeList_PrintList,button_PropertyTypeList_NewPropertyType,btnRefresh,btnHelp"
							tooltiptext="${c:l('button_PropertyTypeList_PropertyTypeSearchDialog.tooltiptext')}" sclass="z-toolbarbutton" />
						<button id="button_PropertyTypeList_PrintList"
							 label = "${c:l('label_Print')}"
							autodisable="button_PropertyTypeList_PropertyTypeSearchDialog,button_PropertyTypeList_PrintList,button_PropertyTypeList_NewPropertyType,btnRefresh,btnHelp"
							tooltiptext="${c:l('button_PropertyTypeList_PrintList.tooltiptext')}" sclass="z-toolbarbutton" />
					</toolbar>
	
					<!-- CRUD BUTTONS -->
					<toolbar align="end"  sclass="toolbar-end">
						<button id="button_PropertyTypeList_NewPropertyType"
							 label = "${c:l('label_New')}"
							autodisable="button_PropertyTypeList_PropertyTypeSearchDialog,button_PropertyTypeList_PrintList,button_PropertyTypeList_NewPropertyType,btnRefresh,btnHelp"
							tooltiptext="${c:l('button_PropertyTypeList_NewPropertyType.tooltiptext')}" sclass="z-toolbarbutton" />
						<button id="btnRefresh" 
							label = "${c:l('label_Refresh')}"
							autodisable="button_PropertyTypeList_PropertyTypeSearchDialog,button_PropertyTypeList_PrintList,button_PropertyTypeList_NewPropertyType,btnRefresh,btnHelp"
							tooltiptext="${c:l('btnRefresh.tooltiptext')}" sclass="z-toolbarbutton" />
						<button id="btnHelp" 
							label = "${c:l('label_Help')}"
							autodisable="button_PropertyTypeList_PropertyTypeSearchDialog,button_PropertyTypeList_PrintList,button_PropertyTypeList_NewPropertyType,btnRefresh,btnHelp"
							tooltiptext="${c:l('btnHelp.tooltiptext')}" sclass="z-toolbarbutton" />
					</toolbar>
				</hbox>
			</div>

			<borderlayout id="borderLayout_PropertyTypeList">
				<north border="none" />
	
				<center border="none">
				<div>
				<grid id="searchGrid"
							sclass="GridLayoutNoBorder" fixedLayout="true">
							<columns>
								<column width="130px" />
								<column width="50px" />
								<column />
								<column width="130px" />
								<column width="50px" />
								<column />
							</columns>
							<rows>
								<row visible = "false">
									<label id="label_PropertyTypeSearch_PropertyTypeId" value="${c:l('label_PropertyTypeSearch_PropertyTypeId.value')}" />
									<listbox id="sortOperator_propertyTypeId" width="43px" mold="select" rows="1" />
									  <uppercasebox  id="propertyTypeId" width="90px" maxlength="10" />
								</row>
								<row>
									<label id="label_PropertyTypeSearch_PropertyTypeName" value="${c:l('label_PropertyTypeSearch_PropertyTypeName.value')}" />
									<listbox id="sortOperator_propertyTypeName" width="43px" mold="select" rows="1" />
									 <textbox id="propertyTypeName" width="250px" maxlength="50" />
								</row>
								<row id="row_AlwWorkflow">
									<label id="label_PropertyTypeSearch_RecordStatus" value="${c:l('label_PropertyTypeSearch_RecordStatus.value')}" />
									<listbox id="sortOperator_recordStatus" width="43px" mold="select" rows="1" />
										<textbox id="recordStatus" width="100px" maxlength="50" />
									<label id="label_PropertyTypeSearch_RecordType" value="${c:l('label_PropertyTypeSearch_RecordType.value')}" />
									<listbox id="sortOperator_recordType" width="43px" mold="select" rows="1" />
										<listbox id="recordType" width="100px" mold="select" />
								</row>
							</rows>
						</grid>
					<listbox id="listBoxPropertyType"  vflex="true" tooltiptext="${c:l('listbox.tooltiptext')}" width="100%" height="100%" multiple="false">
						<listhead sizable="true">
							<listheader id="listheader_PropertyTypeName" label="${c:l('listheader_PropertyTypeName.label')}" sort="auto" 
								width="100%" />
							<listheader id="listheader_RecordStatus"
								label="${c:l('listheader_RecordStatus.label')}" sort="auto"
								width="0%" />
							<listheader id="listheader_RecordType"
								label="${c:l('listheader_RecordType.label')}" sort="auto"
								width="0%" />
						</listhead>		
			
					</listbox>
					</div>
				</center>
				<south border="none" >
					<paging id="pagingPropertyTypeList" />
				</south>
				
			</borderlayout>
	</window>
</zk>