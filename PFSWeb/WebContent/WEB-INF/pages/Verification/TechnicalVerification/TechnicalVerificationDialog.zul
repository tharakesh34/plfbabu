<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ca="http://www.zkoss.org/2005/zk/client/attribute"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="window_TechnicalVerificationDialog" border="normal"
		apply="${technicalVerificationDialogCtrl}" width="100%" height="100%"
		sizable="true">

		<borderlayout id="borderlayoutTechnicalVerification">
			<north id="north">
				<div sclass="z-toolbar" style="padding:0">
					<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
						width="100%" widths="35%,30%,35%">
						<!-- COMMON BUTTONS -->
						<toolbar align="start" sclass="toolbar-start">
							<button id="btnNew"
								tooltiptext="${c:l('btnNew.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
								sclass="z-toolbarbutton" />
							<button id="btnEdit"
								tooltiptext="${c:l('btnEdit.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
								sclass="z-toolbarbutton" />
							<button id="btnDelete"
								tooltiptext="${c:l('btnDelete.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
								sclass="z-toolbarbutton" />
							<button id="btnSave"
								tooltiptext="${c:l('btnSave.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
								sclass="z-toolbarbutton" />
							<button id="btnCancel"
								tooltiptext="${c:l('btnCancel.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
								sclass="z-toolbarbutton" />
							<button id="btnNotes"
								tooltiptext="${c:l('btnNotes.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
								sclass="z-toolbarbutton" />
						</toolbar>
						<!-- SEARCH AREA -->
						<toolbar align="center"
							sclass="toolbar-center">
							<label
								value="${c:l('window_TechnicalVerificationDialog.title')}"
								sclass="label-heading" />
						</toolbar>
						<!-- CRUD BUTTONS -->
						<toolbar align="end" sclass="toolbar-end">
							<button id="btnHelp"
								tooltiptext="${c:l('btnHelp.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
								sclass="z-toolbarbutton" visible="false" />
							<button id="btnClose"
								tooltiptext="${c:l('btnClose.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
								sclass="z-toolbarbutton" />
						</toolbar>
					</hbox>
				</div>
			</north>
			<center border="none" autoscroll="true">
				<div width="100%" height="100%">
					<tabbox width="100%" height="100%">
						<tabs id="tabsIndexCenter">
							<tab id="verificationDetails"
								label="${c:l('label_FieldInvestigationDialog_VerificationDetails.value')}" />
							<tab id="extendedDetailsTab" />
						</tabs>
						<tabpanels id="tabpanelsBoxIndexCenter">
							<tabpanel style="overflow:auto"
								ca:data-scrollable="true">
								<!-- Basic Details -->
								<groupbox id="gb_basicDetails"
									sclass="gb-dialog">
									<caption
										label="${c:l('gb_CustomerDetails')}" />
									<grid sclass="GridLayoutNoBorder"
										fixedLayout="true" style="border:0px">
										<columns>
											<column width="15%" />
											<column width="35%" />
											<column width="15%" />
											<column width="35%" />
										</columns>
										<rows>
											<row>
												<label
													id="label_CustCIF"
													value="${c:l('label_TechnicalVerificationDialog_CustCIF.value')}" />
												<hbox>
													<space
														id="space_CustCIF" spacing="2px" />
													<textbox
														id="custCIF" width="155px" readonly="true" />
												</hbox>
												<label
													id="label_FinReference"
													value="${c:l('label_TechnicalVerificationDialog_FinReference.value')}" />
												<hbox>
													<space
														id="space_FinReference" spacing="2px" />
													<textbox
														id="finReference" width="155px" readonly="true" />
												</hbox>
											</row>
											<row>
												<label
													id="label_CustName"
													value="${c:l('label_TechnicalVerificationDialog_CustName.value')}" />
												<hbox>
													<space
														id="space_CustName" spacing="2px" />
													<textbox
														id="custName" width="155px" readonly="true" />
												</hbox>
												<label
													id="label_CollateralType"
													value="${c:l('label_TechnicalVerificationDialog_CollateralType.value')}" />
												<hbox>
													<space
														id="space_CollateralType" spacing="2px" />
													<textbox
														id="collateralType" width="155px" readonly="true" />
												</hbox>
											</row>
											<row>
												<label
													id="label_ContactNumber1"
													value="${c:l('label_TechnicalVerificationDialog_ContactNumber1.value')}" />
												<hbox>
													<space
														id="space_ContactNumber1" spacing="2px" />
													<textbox
														id="contactNumber1" width="155px" readonly="true" />
												</hbox>
												<label
													id="label_ContactNumber2"
													value="${c:l('label_TechnicalVerificationDialog_ContactNumber2.value')}" />
												<hbox>
													<space
														id="space_ContactNumber2" spacing="2px" />
													<textbox
														id="contactNumber2" width="155px" readonly="true" />
												</hbox>
											</row>
											<row>
												<label
													id="label_CollateralReference"
													value="${c:l('label_TechnicalVerificationDialog_CollateralReference.value')}" />
												<hbox>
													<space
														id="space_CollateralReference" spacing="2px" />
													<textbox
														id="collateralReference" width="155px" readonly="true" />
													<button
														id="btnSearchCollateralRef"
														tooltiptext="${c:l('label_CollateralSearch_CollateralRef.tooltiptext')}"
														image="/images/icons/more.png" />
												</hbox>
											</row>
										</rows>
									</grid>
								</groupbox>

								<!-- Observations -->
								<groupbox sclass="gb-dialog">
									<caption
										label="${c:l('gb_Observations')}" />
									<tabbox width="100%"
										height="100%">
										<tabs
											style="width:0px;height:0px;border:none;">
											<tab
												style="background:none;padding:0px;height:0px;" />
										</tabs>
										<tabpanels>
											<tabpanel
												id="observationsFieldTabPanel" style="overflow:auto" />
										</tabpanels>
									</tabbox>
								</groupbox>


								<!-- Summary -->
								<groupbox id="gb_summary"
									sclass="gb-dialog">
									<caption
										label="${c:l('gb_Summary')}" />
									<grid sclass="GridLayoutNoBorder"
										fixedLayout="true" style="border:0px">
										<columns>
											<column width="15%" />
											<column width="35%" />
											<column width="15%" />
											<column width="35%" />
										</columns>
										<rows>
											<row>
												<label
													id="label_VerificationDate"
													value="${c:l('label_TechnicalVerificationDialog_VerificationDate.value')}" />
												<hbox>
													<space
														id="space_VerificationDate" spacing="2px"
														sclass="mandatory" />
													<datebox
														id="verificationDate" width="155px" />
												</hbox>
												<label
													id="label_AgentCode"
													value="${c:l('label_TechnicalVerificationDialog_AgentCode.value')}" />
												<hbox>
													<space
														id="space_AgentCode" spacing="2px" sclass="mandatory" />
													<uppercasebox
														id="agentCode" width="155px" />
												</hbox>
											</row>
											<row>
												<label
													id="label_AgentName"
													value="${c:l('label_TechnicalVerificationDialog_AgentName.value')}" />
												<hbox>
													<space
														id="space_AgentName" spacing="2px" sclass="mandatory" />
													<textbox
														id="agentName" width="155px" />
												</hbox>
												<label
													id="label_Recommendations"
													value="${c:l('label_TechnicalVerificationDialog_Recommendations.value')}" />
												<hbox>
													<space
														id="space_Recommendations" spacing="2px"
														sclass="mandatory" />
													<combobox
														id="recommendations" width="155px" readonly="true" />
												</hbox>
											</row>
											<row>
												<label id="label_Reason"
													value="${c:l('label_TechnicalVerificationDialog_Reason.value')}" />
												<hbox>
													<extendedcombobox
														id="reason" />
												</hbox>
												<label
													id="label_summaryRemarks"
													value="${c:l('label_TechnicalVerificationDialog_Remarks.value')}" />
												<hbox>
													<space
														id="space_AgentRemarks" spacing="2px" />
													<textbox
														id="summaryRemarks" width="155px" />
												</hbox>
											</row>
											<row>
												<label
													id="label_ValuationAmount"
													value="${c:l('label_TechnicalVerificationDialog_ValuationAmount.value')}" />
												<currencyBox
													id="valuationAmount" />
											</row>
										</rows>
									</grid>
								</groupbox>
							</tabpanel>

							<!-- extendedFieldTabpanel -->
							<tabpanel id="extendedFieldTabpanel"
								style="overflow:auto" />
						</tabpanels>
					</tabbox>
				</div>
			</center>
			<south id="south" sclass="south-dialog">
				<groupbox id="groupboxWf" width="99%">
					<caption label="${c:l('Action')}" />
					<hbox width="95%">
						<radiogroup id="userAction" name="action" />
						<label id="label_RecordStatus"
							value="${c:l('label_South_RecordStatus.value')}"
							sclass="label-status" />
						<label id="recordStatus" />
					</hbox>
				</groupbox>
			</south>
		</borderlayout>
	</window>
</zk>
