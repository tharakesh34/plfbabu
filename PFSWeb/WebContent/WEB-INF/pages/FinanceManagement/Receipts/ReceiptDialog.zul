						tooltiptext="${c:l('btnReceipt.tooltiptext')}"
						autodisable="btnReceipt,btnChangeReceipt,btnCalcReceipts,btnNotes"
						sclass="z-toolbarbutton" />
					<button id="btnChangeReceipt"
						label="${c:l('btnChangeReceipt.label')}"
