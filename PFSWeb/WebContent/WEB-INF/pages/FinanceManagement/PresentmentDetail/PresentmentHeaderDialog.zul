<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">
	
	<window id="window_PresentmentHeaderDialog" border="normal"
			apply="${presentmentHeaderDialogCtrl}" width="100%" height="100%" sizable="false">

		<borderlayout id="borderlayoutPresentmentHeader">
			<north>
				<div sclass="z-toolbar" style="padding:0">
					<hbox pack="stretch" sclass="hboxRemoveWhiteStrips" width="100%" widths="35%,30%,35%">
						<!-- COMMON BUTTONS -->
						<toolbar align="start" style="float:left; border-style: none;">
							<button id="btnNew" tooltiptext="${c:l('btnNew.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" sclass="z-toolbarbutton" visible="false"/>
							<button id="btnEdit" tooltiptext="${c:l('btnEdit.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" sclass="z-toolbarbutton" visible="false"/>
							<button id="btnDelete" tooltiptext="${c:l('btnDelete.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" sclass="z-toolbarbutton" visible="false"/>
							<button id="btnSave" tooltiptext="${c:l('btnSave.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" sclass="z-toolbarbutton" visible="false"/>
							<button id="btnCancel" tooltiptext="${c:l('btnCancel.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" sclass="z-toolbarbutton" visible="false"/>
							<button id="btnNotes" tooltiptext="${c:l('btnNotes.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" sclass="z-toolbarbutton" visible="false"/>
						</toolbar>
				<!-- SEARCH AREA -->
						<toolbar align="center" style="toolbar-center">
							<label value="${c:l('window_PresentmentHeaderDialog.title')}" sclass="label-heading" />
						</toolbar>
						<!-- CRUD BUTTONS -->
						<toolbar align="end" style="float:right; border-style: none;">
							<button id="btnHelp" tooltiptext="${c:l('btnHelp.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" sclass="z-toolbarbutton" visible="false"/>
							<button id="btnClose" tooltiptext="${c:l('btnClose.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" sclass="z-toolbarbutton" />
						</toolbar>
					</hbox>
				</div>
			</north>
			<center border="none" autoscroll="false"
					style="padding:5px;">
					<div>
						<grid id="searchGrid"
							sclass="GridLayoutNoBorder" fixedLayout="true"
							style=" border-style: none; padding-left:5px; padding-right:5px;">
							<columns>
									<column width="160px" />
									<column />
									<column width="160px" />
									<column />
									<column width="160px" />
									<column />
								</columns>
							<rows>
								<row>
									<label value="${c:l('label_PresentmentDetailList_Bank.value')}" />
									<extendedcombobox id="partnerBank" />
									
									<label value="${c:l('label_PresentmentDetailList_PresentmentReference.value')}" />
									<label id="label_PresentmentReference"  style="font-weight:bold;padding-right:12px;"/>
									
									<label value="${c:l('label_PresentmentDetailList_PresentmentStatus.value')}" />
									<label id="label_PresentmentStatus" style="font-weight:bold;padding-right:12px;"/>
								</row>
							</rows>
						</grid>
						<space height="10px"></space>
						<grid id="dBStatusGrid" visible="false"
							sclass="GridLayoutNoBorder" fixedLayout="true"
							style=" border-style: none; padding-left:5px; padding-right:5px;">
							<columns>
									<column width="160px" />
									<column />
									<column width="160px" />
									<column />
									<column width="160px" />
									<column />
							</columns>
							<rows>
								<row>
									<label value="Total Presentments" />
									<label id="label_TotalPresentments" style="font-weight:bold;padding-right:12px;"/>

									<label value="Success Presentments" />
									<label id="label_SuccessPresentments" style="font-weight:bold;padding-right:12px;"/>
									
									<label value="Failed Presentments" />
									<label id="label_FailedPresentments" style="font-weight:bold;padding-right:12px;"/>
								</row>
							</rows>
						</grid>

						<tabbox width="100%">
							<tabs id="tabsIndexCenter">
								<tab
									label="${c:l('tab_PresentmentDetailDialog_Include.value')}"
									id="includeTab" />
								<tab
									label="${c:l('tab_PresentmentDetailDialog_ManualExclude.value')}"
									id="manualExcludeTab" />
								<tab
									label="${c:l('tab_PresentmentDetailDialog_AutoExclude.value')}"
									id="autoExcludeTab" />
							</tabs>
							<tabpanels>
								<tabpanel>
									<groupbox width="100%"
										height="100%">
										<div sclass="z-toolbar"
											style="padding:0">
											<hbox pack="stretch"
												sclass="hboxRemoveWhiteStrips" style=" padding:0px;"
												height="100%" width="100%">
												<toolbar align="start"
													style="border:none;" />
												<toolbar align="end"
													style="float:right; border-style: none; padding:0px;"
													height="100%">
													<button
														id="btn_AddExlude" label="${c:l('label_AddExclude')}"
														tooltiptext="${c:l('btn_AddExclude.tooltiptext')}"
														autodisable="btn_AddExlude" />
												</toolbar>
											</hbox>
										</div>
										<listbox id="listBox_Include"
											emptyMessage="${c:l('listbox.emptyMessage')}" width="100%"
											height="100%" sizedByContent="true" span="true"
											multiple="true" checkmark="true" mold="paging"
											autopaging="true">
											<listhead sizable="true">
												<listheader
													label="${c:l('listheader_PresentmentDetail_Customer.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_LoanReference.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_LoanTypeOrProduct.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_EmiDate.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_Amount.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_MandateType.label')}"
													hflex="min" />
											</listhead>
										</listbox>
									</groupbox>
								</tabpanel>
								<tabpanel>
									<groupbox width="100%"
										height="100%">
										<div sclass="z-toolbar"
											style="padding:0">
											<hbox pack="stretch"
												sclass="hboxRemoveWhiteStrips" style=" padding:0px;"
												height="100%" width="100%">
												<toolbar align="start"
													style="border:none;" />
												<toolbar align="end"
													style="float:right; border-style: none; padding:0px;"
													height="100%">
													<button
														id="btn_AddInclude" label="${c:l('label_AddInclude')}"
														tooltiptext="${c:l('btn_AddInclude.tooltiptext')}"
														autodisable="btn_AddInclude" />
												</toolbar>
											</hbox>
										</div>
										<listbox
											id="listBox_ManualExclude"
											emptyMessage="${c:l('listbox.emptyMessage')}"  width="100%"
											height="100%" sizedByContent="true" span="true"
											multiple="true" checkmark="true" mold="paging"
											autopaging="true">
											<listhead sizable="true">
												<listheader
													label="${c:l('listheader_PresentmentDetail_Customer.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_LoanReference.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_LoanTypeOrProduct.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_EmiDate.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_Amount.label')}"
													hflex="min" />
												<listheader
													id="listheader_PresentmentDetail_Exclude_MandateType"
													label="${c:l('listheader_PresentmentDetail_MandateType.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_ExcludeStatus.label')}"
													hflex="min" align="center" />
											</listhead>
										</listbox>
									</groupbox>
								</tabpanel>

								<tabpanel>
									<groupbox width="100%"
										height="100%">
										<div sclass="z-toolbar"
											style="padding:0">
											<hbox pack="stretch"
												sclass="hboxRemoveWhiteStrips" style=" padding:0px;"
												height="100%" width="100%">
												<toolbar align="start"
													style="border:none;">
												</toolbar>
												<toolbar align="end"
													style="float:right; border-style: none; padding:0px;"
													height="100%">
												</toolbar>
											</hbox>
										</div>
										<listbox
											id="listBox_AutoExclude"
											emptyMessage="${c:l('listbox.emptyMessage')}" width="100%"
											height="100%" sizedByContent="true" span="true" mold="paging"
											autopaging="true">
											<listhead sizable="true">
												<listheader
													label="${c:l('listheader_PresentmentDetail_Customer.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_LoanReference.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_LoanTypeOrProduct.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_EmiDate.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_Amount.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_MandateType.label')}"
													hflex="min" />
												<listheader
													label="${c:l('listheader_PresentmentDetail_ExcludeStatus.label')}"
													hflex="min" align="center" />
											</listhead>
										</listbox>
									</groupbox>
								</tabpanel>
							</tabpanels>
						</tabbox>
					</div>
				</center>
		<south sclass="south-dialog">
			<groupbox id="groupboxWf" width="99%">
				<caption label="${c:l('Action')}" />
				<hbox width="95%">
					<radiogroup id="userAction" name="action" />
					<label id="label_RecordStatus"  value="${c:l('label_South_RecordStatus.value')}" sclass="label-status" visible="false"/>
					<label id="recordStatus"  visible="false"/>
				</hbox>
			</groupbox>
		</south>
	</borderlayout>
</window>
</zk> 