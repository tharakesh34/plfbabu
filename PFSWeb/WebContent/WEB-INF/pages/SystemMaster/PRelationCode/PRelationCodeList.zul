<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window>
		<window id="window_PRelationCodeList"
			apply="${pRelationCodeListCtrl}" border="none" width="100%">

			<div sclass="z-toolbar" style="padding:0px" height="28px">
				<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
					width="100%" widths="30%,40%,30%">

					<!-- COMMON BUTTONS -->
					<label value="" />

					<!-- SEARCH AREA -->
					<toolbar align="center" sclass="toolbar-center">
						<button
							id="button_PRelationCodeList_PRelationCodeSearchDialog"
							 label = "${c:l('label_Filter')}"
							autodisable="button_PRelationCodeList_PRelationCodeSearchDialog,button_PRelationCodeList_PrintList,button_PRelationCodeList_NewPRelationCode,btnRefresh,btnHelp"
							tooltiptext="${c:l('button_PRelationCodeList_PRelationCodeSearchDialog.tooltiptext')}" sclass="z-toolbarbutton" />
						<button id="button_PRelationCodeList_PrintList"
							 label = "${c:l('label_Print')}"
							autodisable="button_PRelationCodeList_PRelationCodeSearchDialog,button_PRelationCodeList_PrintList,button_PRelationCodeList_NewPRelationCode,btnRefresh,btnHelp"
							tooltiptext="${c:l('button_PRelationCodeList_PrintList.tooltiptext')}" sclass="z-toolbarbutton" />
					</toolbar>

					<!-- CRUD BUTTONS -->
					<toolbar align="end"  sclass="toolbar-end">
						<button
							id="button_PRelationCodeList_NewPRelationCode" 
							label = "${c:l('label_New')}"
							autodisable="button_PRelationCodeList_PRelationCodeSearchDialog,button_PRelationCodeList_PrintList,button_PRelationCodeList_NewPRelationCode,btnRefresh,btnHelp"
							tooltiptext="${c:l('button_PRelationCodeList_NewPRelationCode.tooltiptext')}" sclass="z-toolbarbutton" />
						<button id="btnRefresh" 
							label = "${c:l('label_Refresh')}"
							autodisable="button_PRelationCodeList_PRelationCodeSearchDialog,button_PRelationCodeList_PrintList,button_PRelationCodeList_NewPRelationCode,btnRefresh,btnHelp"
							tooltiptext="${c:l('btnRefresh.tooltiptext')}" sclass="z-toolbarbutton" />
						<button id="btnHelp" 
							label = "${c:l('label_Help')}"
							autodisable="button_PRelationCodeList_PRelationCodeSearchDialog,button_PRelationCodeList_PrintList,button_PRelationCodeList_NewPRelationCode,btnRefresh,btnHelp"
							tooltiptext="${c:l('btnHelp.tooltiptext')}" sclass="z-toolbarbutton" />
					</toolbar>
				</hbox>
			</div>

			<borderlayout id="borderLayout_PRelationCodeList">
				<north border="none" />
				<center border="none">
					<div>
						<grid id="searchGrid"
							sclass="GridLayoutNoBorder" fixedLayout="true"
							style="border:0px; padding-left:5px; padding-right:5px;">
							<columns>
								<column width="130px" />
								<column width="50px" />
								<column />
								<column width="130px" />
								<column width="50px" />
								<column />
							</columns>
							<rows>
								<row>
									<label
										id="label_PRelationCodeSearch_PRelationCode"
										value="${c:l('label_PRelationCodeSearch_PRelationCode.value')}" />
									<listbox
										id="sortOperator_pRelationCode" width="43px" mold="select"
										rows="1" />
									<uppercasebox id="pRelationCode"
										width="100px" maxlength="8" />
									<label
										id="label_PRelationCodeSearch_PRelationDesc"
										value="${c:l('label_PRelationCodeSearch_PRelationDesc.value')}" />
									<listbox
										id="sortOperator_pRelationDesc" width="43px" mold="select"
										rows="1" />
									<textbox id="pRelationDesc"
										width="320px" maxlength="50" />
								</row>
								<row>
									<label
										id="label_PRelationCodeSearch_RelationCodeIsActive"
										value="${c:l('label_PRelationCodeSearch_RelationCodeIsActive.value')}" />
									<listbox
										id="sortOperator_relationCodeIsActive" width="43px"
										mold="select" rows="1" />
									<checkbox id="relationCodeIsActive"
										checked="false" />
								</row>
								<row id="row_AlwWorkflow">
									<label
										id="label_PRelationCodeSearch_RecordStatus"
										value="${c:l('label_PRelationCodeSearch_RecordStatus.value')}" />
									<listbox
										id="sortOperator_recordStatus" width="43px" mold="select"
										rows="1" />
									<textbox id="recordStatus"
										width="100px" maxlength="50" />
									<label
										id="label_PRelationCodeSearch_RecordType"
										value="${c:l('label_PRelationCodeSearch_RecordType.value')}" />
									<listbox
										id="sortOperator_recordType" width="43px" mold="select"
										rows="1" />
									<listbox id="recordType"
										width="100px" mold="select" />
								</row>
							</rows>
						</grid>
						<listbox id="listBoxPRelationCode" vflex="true"
							tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
							height="100%" multiple="false" span="true">
							<listhead sizable="true">
								<listheader
									id="listheader_PRelationCode"
									label="${c:l('listheader_PRelationCode.label')}" sort="auto"
									hflex="min" />
								<listheader
									id="listheader_PRelationDesc"
									label="${c:l('listheader_PRelationDesc.label')}" sort="auto"
									hflex="min" />
								<listheader
									id="listheader_RelationCodeIsActive"
									label="${c:l('listheader_RelationCodeIsActive.label')}"
									sort="auto" hflex="min" />
								<listheader id="listheader_RecordStatus"
									label="${c:l('listheader_RecordStatus.label')}" sort="auto"
									hflex="min" />
								<listheader id="listheader_RecordType"
									label="${c:l('listheader_RecordType.label')}" sort="auto"
									hflex="min" />
							</listhead>
						</listbox>
					</div>
				</center>
				<south border="none">
					<paging id="pagingPRelationCodeList" />
				</south>

			</borderlayout>
		</window>
	</window>
</zk>