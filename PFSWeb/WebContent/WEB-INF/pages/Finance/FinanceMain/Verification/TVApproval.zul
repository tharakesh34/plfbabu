<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window>
		<window id="window_TVerificationDialog" border="none"
			apply="${tVerificationDialogCtrl}" width="100%" height="100%"
			style="padding-top:10px" sizable="false">
			<radiogroup id="tv" visible="false" />

			<borderlayout height="100%">
				<north border="none">
					<groupbox id="finBasicdetails" zclass="null" />
				</north>
				<center sclass="none" autoscroll="true">
					<div>
						<div>
						<listbox id="listBoxTechnicalVerification" width="100%" span="true" style="overflow:auto;" emptyMessage="${c:l('listbox.emptyMessage')}" sizedByContent="true">
								<listhead sizable="false">
									<listheader
										id="listheader_TechnicalVerification_Select"
										label="${c:l('listheader_TechnicalVerification_Select.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_CollateralType"
										label="${c:l('listheader_TechnicalVerification_CollateralType.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_DepositorCIF"
										label="${c:l('listheader_TechnicalVerification_DepositorCIF.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_DepositorName"
										label="${c:l('listheader_TechnicalVerification_DepositorName.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_CollateralReference"
										label="${c:l('listheader_TechnicalVerification_CollateralReference.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_TV"
										label="${c:l('listheader_TechnicalVerification_TV.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_Agency"
										label="${c:l('listheader_TechnicalVerification_Agency.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_Reason"
										label="${c:l('listheader_TechnicalVerification_Reason.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_Remarks"
										label="${c:l('listheader_TechnicalVerification_Remarks.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_Status"
										label="${c:l('listheader_TechnicalVerification_Status.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_VrfDate"
										label="${c:l('listheader_TechnicalVerification_VrfDate.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_Decision"
										label="${c:l('listheader_TechnicalVerification_Decision.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_ReInitAgency"
										label="${c:l('listheader_TechnicalVerification_Agency.label')}"
										sort="false"/>
									<listheader
										id="listheader_TechnicalVerification_ReInitRemarks"
										label="${c:l('listheader_TechnicalVerification_Remarks.label')}"
										sort="false"/>
								</listhead>
							</listbox>
						</div>
						<div>
							<groupbox id="tvInquiry" zclass="null"
								height="330px" />
						</div>
					</div>
				</center>
			</borderlayout>
		</window>
	</window>
</zk>
