<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="window_MiscPostingUploadDialog" border="normal"
		apply="${miscPostingUploadDialogCtrl}" width="100%" height="100%"
		sizable="false">

		<borderlayout id="borderlayoutRefundUpload">
			<north>
				<div sclass="z-toolbar" style="padding:0">
					<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
						width="100%" widths="35%,30%,35%">

						<!-- COMMON BUTTONS -->
						<toolbar align="start" sclass="toolbar-start">
							<button id="btnNew"
								tooltiptext="${c:l('btnNew.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload"
								sclass="z-toolbarbutton" />
							<button id="btnEdit"
								tooltiptext="${c:l('btnEdit.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload"
								sclass="z-toolbarbutton" />
							<button id="btnDelete"
								tooltiptext="${c:l('btnDelete.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload"
								sclass="z-toolbarbutton" />
							<button id="btnSave"
								tooltiptext="${c:l('btnSave.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload"
								sclass="z-toolbarbutton" />
							<button id="btnCancel"
								tooltiptext="${c:l('btnCancel.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload"
								sclass="z-toolbarbutton" />
							<button id="btnNotes"
								tooltiptext="${c:l('btnNotes.tooltiptext')}" visible="false"
								sclass="z-toolbarbutton" />
						</toolbar>

						<!-- SEARCH AREA -->
						<toolbar align="center"
							sclass="toolbar-center">
							<label
								value="${c:l('window_MiscPostingUploadDialog.title')}"
								sclass="label-heading" />
						</toolbar>

						<!-- CRUD BUTTONS -->
						<toolbar align="end" sclass="toolbar-end">
							<button id="btnHelp" visible="false"
								label="${c:l('label_Help')}"
								tooltiptext="${c:l('btnHelp.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload"
								sclass="z-toolbarbutton" />
							<button id="btnClose"
								tooltiptext="${c:l('btnClose.tooltiptext')}"
								autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload"
								sclass="z-toolbarbutton" />
						</toolbar>
					</hbox>
				</div>


			</north>
			<center border="none" autoscroll="false">
				<div>
					<groupbox id="gb_basicDetails" sclass="gb-dialog">
						<grid sclass="GridLayoutNoBorder"
							id="grid_basicDetails" fixedLayout="true" style="border:0px">
							<columns>
								<column width="200px" />
								<column />
							</columns>
							<rows>
								<row>
									<label
										id="label_MiscPostingUploadDialog_Entity"
										value="${c:l('label_MiscPostingUploadDialog_Entity.value')}" />
									<hbox>
										<space spacing="2px" sclass="" />
										<extendedcombobox id="entity"
											readonly="true" />
									</hbox>
								</row>
								<row>
									<label
										id="label_MiscPostingUploadDialog_Filename"
										value="${c:l('label_MiscPostingUploadDialog_Filename.value')}" />
									<hbox>
										<space spacing="2px" sclass="" />
										<textbox id="txtFileName"
											width="250px" readonly="true" />
									</hbox>
								</row>
							</rows>
						</grid>
					</groupbox>
					<tabbox width="100%">
						<tabs>
							<tab
								label="${c:l('label_MiscPostingUploadDialog_SuccessRecords.value')}"
								id="tabSuccessRecords" />
							<tab
								label="${c:l('label_MiscPostingUploadDialog_FailedRecords.value')}"
								id="tabFailedRecords" />
							<tab
								label="${c:l('label_MiscPostingUploadDialog_RejectRecords.value')}"
								id="tabRejectRecords" visible="false" />
						</tabs>
						<tabpanels>
							<tabpanel style="overflow:auto">
								<div sclass="z-toolbar"
									style="padding:0">
									<hbox pack="stretch"
										sclass="hboxRemoveWhiteStrips" style=" padding:0px;"
										height="100%" width="100%">
										<toolbar align="start"
											style="border:none;" />
										<toolbar align="end"
											sclass="toolbar-end">
											<button
												id="approval_RejectButton" label="Add To Reject List"
												visible="false"
												autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload,approval_SuccessButton" />
										</toolbar>
									</hbox>
								</div>
								<listbox id="listBoxUploadDetails"
									emptyMessage="${c:l('listbox.emptyMessage')}" width="100%"
									height="100%" sizedByContent="true" span="true" multiple="true"
									checkmark="true" mold="paging" autopaging="true">
									<listhead sizable="true">

										<listheader
											id="listheader_MiscPostingUploadDialog_TransactionId"
											label="${c:l('listheader_MiscPostingUploadDialog_TransactionId.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_BatchPurpose"
											label="${c:l('listheader_MiscPostingUploadDialog_BatchPurpose.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Branch"
											label="${c:l('listheader_MiscPostingUploadDialog_Branch.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_PostingAgainst"
											label="${c:l('listheader_MiscPostingUploadDialog_PostingAgainst.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Reference"
											label="${c:l('listheader_MiscPostingUploadDialog_Reference.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Account"
											label="${c:l('listheader_MiscPostingUploadDialog_Account.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_DebitCr"
											label="${c:l('listheader_MiscPostingUploadDialog_DebitCr.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_PostingAmount"
											label="${c:l('listheader_MiscPostingUploadDialog_PostingAmount.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_ValueDate"
											label="${c:l('listheader_MiscPostingUploadDialog_ValueDate.label')}"
											sort="false" align="right" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Reason"
											label="${c:l('listheader_MiscPostingUploadDialog_Reason.label')}"
											sort="false" hflex="min" visible="false" />
									</listhead>
								</listbox>
							</tabpanel>
							<tabpanel style="overflow:auto">
								<listbox id="listBoxUploadFailedDetails"
									emptyMessage="${c:l('listbox.emptyMessage')}"
									tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
									height="100%" sizedByContent="true" span="true"
									multiple="false" checkmark="false" mold="paging"
									autopaging="true">
									<listhead sizable="true">

										<listheader
											id="listheader_MiscPostingUploadDialog_TransactionId1"
											label="${c:l('listheader_MiscPostingUploadDialog_TransactionId.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_BatchPurpose1"
											label="${c:l('listheader_MiscPostingUploadDialog_BatchPurpose.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Branch1"
											label="${c:l('listheader_MiscPostingUploadDialog_Branch.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_PostingAgainst1"
											label="${c:l('listheader_MiscPostingUploadDialog_PostingAgainst.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Reference1"
											label="${c:l('listheader_MiscPostingUploadDialog_Reference.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Account1"
											label="${c:l('listheader_MiscPostingUploadDialog_Account.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_DebitCr1"
											label="${c:l('listheader_MiscPostingUploadDialog_DebitCr.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_PostingAmount1"
											label="${c:l('listheader_MiscPostingUploadDialog_PostingAmount.label')}"
											sort="false" align="right" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_ValueDate1"
											label="${c:l('listheader_MiscPostingUploadDialog_ValueDate.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Reason1"
											label="${c:l('listheader_MiscPostingUploadDialog_Reason.label')}"
											sort="false" hflex="min" />
									</listhead>
								</listbox>
							</tabpanel>
							<tabpanel style="overflow:auto">
								<div sclass="z-toolbar"
									style="padding:0">
									<hbox pack="stretch"
										sclass="hboxRemoveWhiteStrips" style=" padding:0px;"
										height="100%" width="100%">
										<toolbar align="start"
											style="border:none;" />
										<toolbar align="end"
											sclass="toolbar-end">
											<button
												id="approval_SuccessButton" label="Add To Success List"
												visible="false"
												autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnBrowse,btndownload,approval_RejectButton" />
										</toolbar>
									</hbox>
								</div>
								<listbox id="listBoxUploadRejectDetails"
									emptyMessage="${c:l('listbox.emptyMessage')}"
									tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
									height="100%" sizedByContent="true" span="true" multiple="true"
									checkmark="true" mold="paging" autopaging="true">
									<listhead sizable="true">
										<listheader
											id="listheader_MiscPostingUploadDialog_TransactionId2"
											label="${c:l('listheader_MiscPostingUploadDialog_TransactionId.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_BatchPurpose2"
											label="${c:l('listheader_MiscPostingUploadDialog_BatchPurpose.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Branch2"
											label="${c:l('listheader_MiscPostingUploadDialog_Branch.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_PostingAgainst2"
											label="${c:l('listheader_MiscPostingUploadDialog_PostingAgainst.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Reference2"
											label="${c:l('listheader_MiscPostingUploadDialog_Reference.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Account2"
											label="${c:l('listheader_MiscPostingUploadDialog_Account.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_DebitCr2"
											label="${c:l('listheader_MiscPostingUploadDialog_DebitCr.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_PostingAmount2"
											label="${c:l('listheader_MiscPostingUploadDialog_PostingAmount.label')}"
											sort="false" align="right" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_ValueDate2"
											label="${c:l('listheader_MiscPostingUploadDialog_ValueDate.label')}"
											sort="false" hflex="min" />
										<listheader
											id="listheader_MiscPostingUploadDialog_Reason2"
											label="${c:l('listheader_MiscPostingUploadDialog_Reason.label')}"
											sort="false" hflex="min" />
									</listhead>
								</listbox>
							</tabpanel>
						</tabpanels>
					</tabbox>
				</div>
			</center>
			<south id="south" sclass="south-dialog">
				<groupbox id="groupboxWf" width="99%"
					closable="false">
					<caption label="${c:l('Action')}" />
					<hbox width="95%">
						<radiogroup id="userAction" name="action" />
						<label id="label_RecordStatus"
							value="${c:l('label_RecordStatus')}" sclass="label-status" />
						<label id="recordStatus" />
					</hbox>
				</groupbox>
			</south>
		</borderlayout>
	</window>
</zk>
