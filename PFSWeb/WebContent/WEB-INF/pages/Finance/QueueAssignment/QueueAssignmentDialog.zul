<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window>

		<window id="window_QueueAssignmentDialog"
			apply="${queueAssignmentDialogCtrl}" border="none" width="100%"
			height="100%" sizable="false">

			<borderlayout id="borderLayout_QueueAssignmentDialog">
				<north border="none">
					<div sclass="z-toolbar" style="padding:0">
						<hbox pack="stretch"
							sclass="hboxRemoveWhiteStrips" width="100%"
							widths="35%,30%,35%">

							<!-- COMMON BUTTONS -->
							<toolbar align="start"
								sclass="toolbar-start">
								<button id="btnNew"
									tooltiptext="${c:l('btnNew.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
									sclass="z-toolbarbutton" />
								<button id="btnEdit"
									tooltiptext="${c:l('btnEdit.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
									sclass="z-toolbarbutton" />
								<button id="btnSave"
									tooltiptext="${c:l('btnSave.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
									sclass="z-toolbarbutton" />
								<button id="btnDelete"
									tooltiptext="${c:l('btnDelete.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
									sclass="z-toolbarbutton" />
								<button id="btnCancel"
									tooltiptext="${c:l('btnCancel.tooltiptext')}"
									sclass="z-toolbarbutton" />
								<button id="btnNotes"
									tooltiptext="${c:l('btnNotes.tooltiptext')}" visible="false"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
									sclass="z-toolbarbutton" />
							</toolbar>
							<!-- SEARCH AREA -->
							<toolbar align="center"
								sclass="toolbar-center">
								<label id="window_AssignmentTitle"
									sclass="label-heading" />
							</toolbar>

							<!-- CRUD BUTTONS -->
							<toolbar align="end" sclass="toolbar-end">
								<button id="btnHelp" visible="false"
									label="${c:l('label_Help')}"
									tooltiptext="${c:l('btnHelp.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
									sclass="z-toolbarbutton" />
								<button id="btnClose"
									tooltiptext="${c:l('btnClose.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose"
									sclass="z-toolbarbutton" />
							</toolbar>
						</hbox>
					</div>
				</north>
				<center border="none">
					<div>
						<groupbox id="gb_basicDetails"
							sclass="gb-dialog">
							<caption
								label="${c:l('label_QueueAssignmentDialog_BasicDetails')}" />
							<grid sclass="GridLayoutNoBorder"
								fixedLayout="true" style="border:0px">
								<columns>
									<column width="260px" />
									<column />
									<column width="260px" />
									<column />
								</columns>
								<rows>
									<row visible="true">
										<label
											id="label_QueueAssignmentDialog_FromUser"
											value="${c:l('label_QueueAssignmentDialog_FromUser.value')}" />
										<hbox>
											<extendedcombobox
												id="fromUser" />

										</hbox>
										<label
											id="label_QueueAssignmentDialog_FromUserRole"
											value="${c:l('label_QueueAssignmentDialog_FromUserRole.value')}" />
										<label id="userRole"
											style="font-weight: bold;" />
									</row>
									<row>
										<label
											value="${c:l('label_QueueAssignmentDialog_AssignAll.value')}" />
										<hbox>
											<extendedcombobox
												id="toUser" />
										</hbox>
									</row>
								</rows>
							</grid>
						</groupbox>
						<groupbox id="gb_Search" sclass="gb-dialog">
							<caption
								label="${c:l('label_QueueAssignmentDialog_Search')}" />
							<grid sclass="GridLayoutNoBorder"
								fixedLayout="true" style="border:0px">
								<columns>
									<column width="200px" />
									<column width="70px" />
									<column width="200px" />
									<column />
								</columns>
								<rows>
									<row visible="true">
										<label
											id="label_QueueAssignmentDialog_FinReference"
											value="${c:l('label_QueueAssignmentDialog_FinReference.value')}" />
										<listbox
											id="sortOperator_finReference" width="43px" mold="select"
											rows="1" />
										<hbox>
											<uppercasebox
												id="finReference" width="170px" visible="true" />
										</hbox>
										</row>
										<row>
										<label
											id="label_QueueAssignmentDialog_CustomerCIF"
											value="${c:l('label_QueueAssignmentDialog_CustomerCIF.value')}" />
										<listbox
											id="sortOperator_CustomerCIF" width="43px" mold="select"
											rows="1" />
										<hbox>
											<uppercasebox id="custCIF"
												width="170px" />
										</hbox>
									</row>
									<row>
										<hbox></hbox>
											<button id="btn_Search"
												label="${c:l('label_Filter')}"
												tooltiptext="${c:l('button_QueueAssignmentDialog_Search.tooltiptext')}" />
											<button id="btn_Refresh"
												label="${c:l('label_Refresh')}"
												tooltiptext="${c:l('button_QueueAssignmentDialogRefresh.tooltiptext')}" />
									</row>
								</rows>
							</grid>
						</groupbox>
						<listbox id="listbox_AssignmentRecords"
							emptyMessage="${c:l('listbox.emptyMessage')}" fixedLayout="true"
							  height="100%" width="100%" multiple="false"
							sizedByContent="true" span="true">
							<listhead sizable="true">
								<listheader id="listheader_FinReference"
									label="${c:l('listheader_QueueAssignmentDialog_Reference.label')}"
									hflex="min" />
								<listheader id="listheader_CustName"
									label="${c:l('listheader_QueueAssignmentDialog_Customer.label')}"
									hflex="min" />
								<listheader id="listheader_FinAmount"
									label="${c:l('listheader_QueueAssignmentDialog_Amount.label')}"
									hflex="min" />
								<listheader id="listheader_ActualOwner"
									label="${c:l('listheader_QueueAssignmentDialog_ActualOwner.label')}"
									hflex="min" />
								<listheader id="listheader_ToUser"
									label="${c:l('listheader_QueueAssignmentDialog_ToUser.label')}"
									hflex="min" />
								<listheader id="listheader_RecordStatus"
									label="${c:l('listheader_QueueAssignmentDialog_RecordStatus.label')}"
									hflex="min" />
							</listhead>
						</listbox>
					</div>
				</center>
				<south style="border:none;">
					<groupbox id="groupboxWf" width="99%"
						closable="false">
						<caption label="${c:l('Action')}" />
						<hbox width="95%">
							<radiogroup id="userAction" name="action" />
							<label
								id="label_QueueAssignmentDialog_RecordStatus"
								value="${c:l('label_South_RecordStatus.value')}"
								sclass="label-status" />
							<label id="recordStatus" />
						</hbox>
					</groupbox>
				</south>
			</borderlayout>
		</window>
	</window>
</zk>