<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>
<zk xmlns="http://www.zkoss.org/2005/zul" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">
	<window>
		<window id="window_GuarantorDetailDialog" border="none" apply="${guarantorDetailDialogCtrl}" width="100%" height="100%" sizable="false">
			<borderlayout id="borderlayoutGuarantorDetail">
				<north>
					<div sclass="z-toolbar" style="padding:0">
						<hbox pack="stretch" sclass="hboxRemoveWhiteStrips" width="100%" widths="35%,30%,35%">
							<!-- COMMON BUTTONS -->
							<toolbar align="start" sclass="toolbar-start">
								<button id="btnNew" visible="false" height="24px" tooltiptext="${c:l('btnNew.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnEdit" visible="false" height="24px" tooltiptext="${c:l('btnEdit.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnDelete" visible="false" height="24px" tooltiptext="${c:l('btnDelete.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnSave" visible="false" height="24px" tooltiptext="${c:l('btnSave.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnCancel" visible="false" height="24px" tooltiptext="${c:l('btnCancel.tooltiptext')}" />
								<button id="btnNotes" visible="false" height="24px" tooltiptext="${c:l('btnNotes.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
							</toolbar>
							<!-- SEARCH AREA -->
							<toolbar align="center" sclass="toolbar-center">
								<label value="${c:l('window_GuarantorDetailDialog.title')}" sclass="label-heading" />
							</toolbar>
							<!-- CRUD BUTTONS -->
							<toolbar align="end" sclass="toolbar-end">
								<button id="btnHelp" height="24px" label="${c:l('label_Help')}" tooltiptext="${c:l('btnHelp.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnClose" height="24px" tooltiptext="${c:l('btnClose.tooltiptext')}" autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
							</toolbar>
						</hbox>
					</div>
				</north>
				<center border="none" autoscroll="true">
					<groupbox>
						<groupbox id="gb_basicDetails" mold="3d">
							<grid sclass="GridLayoutNoBorder" fixedLayout="true" style="border:0px">
								<columns>
									<column width="20%" />
									<column width="30%" />
									<column width="20%" />
									<column width="30%" />
								</columns>
								<rows>
									<row id="row0">
										<label id="label_BankCustomer" value="${c:l('label_GuarantorDetailDialog_BankCustomer.value')}" />
										<hlayout id="hlayout_BankCustomer">
											<space id="space_BankCustomer" spacing="2px" />
											<checkbox id="bankCustomer" checked="false" />
										</hlayout>
									</row>
									<row id="row1">
										<label id="label_GuarantorIDType" value="${c:l('label_GuarantorDetailDialog_GuarantorIDType.value')}" />
										<hlayout id="hlayout_GuarantorIDType">
											<space id="space_GuarantorIDType" spacing="2px" sclass="mandatory" />
											<combobox id="guarantorIDType" width="200px" readonly="true" />
										</hlayout>
										<label id="label_GuranteePercentage" value="${c:l('label_GuarantorDetailDialog_GuranteePercentage.value')}" />
										<hlayout id="hlayout_GuranteePercentage">
											<space id="space_GuranteePercentage" spacing="2px" sclass="mandatory" />
											<decimalbox id="guranteePercentage" width="100px" maxlength="7" />
										</hlayout>
									</row>
									<row id="row2">
										<label id="label_GuarantorCIF" value="${c:l('label_GuarantorDetailDialog_GuarantorCIF/ID.value')}" />
										<hlayout spacing="0px">
											<hlayout id="hlayout_GuarantorCIF">
												<space id="space_GuarantorCIF" spacing="2px" sclass="mandatory" />
												<hlayout spacing="0px">
													<textbox id="guarantorCIF" width="200px" visible="true" />
													<button id="btnSearchGuarantorCIF" image="/images/icons/LOVSearch.png" height="24px" />
												</hlayout>
											</hlayout>
											<hlayout id="hlayout_GuarantorIDNumber">
												<space id="space_GuarantorIDNumber" spacing="2px" sclass="mandatory" />
												<textbox id="guarantorIDNumber" maxlength="50" width="200px" />
											</hlayout>
										</hlayout>
										<label id="label_Name" value="${c:l('label_GuarantorDetailDialog_Name.value')}" />
										<hlayout id="hlayout_Name">
											<space id="space_Name" spacing="2px" sclass="mandatory" />
											<textbox id="guarantorCIFName" maxlength="100" width="200px" />
										</hlayout>
									</row>
									<row id="row3">
										<label id="label_MobileNo" value="${c:l('label_GuarantorDetailDialog_MobileNo.value')}" />
										<hlayout id="hlayout_MobileNo">
											<space id="space_MobileNo" spacing="2px" sclass="mandatory" />
											<textbox id="mobileNo" maxlength="25" width="200px" />
										</hlayout>
										<label id="label_EmailId" value="${c:l('label_GuarantorDetailDialog_EmailId.value')}" />
										<hlayout id="hlayout_EmailId">
											<space id="space_EmailId" spacing="2px" />
											<textbox id="emailId" maxlength="200" width="200px" />
										</hlayout>
									</row>
									<row id="row4">
										<label id="label_GuarantorProof" value="${c:l('label_GuarantorDetailDialog_GuarantorProof.value')}" />
										<hlayout id="hlayout_GuarantorProof">
											<space id="space_GuarantorProof" spacing="2px" sclass="mandatory" />
											<textbox id="guarantorProofName" maxlength="500" width="200px" readonly="true" disabled="true"/>
											<textbox id="guarantorProof" maxlength="1073741823" multiline="true" width="200px" visible="false" readonly="true"/>
											<button label="${c:l('label_GuarantorUploadDialog_btnUpload.value')}" upload="true" id="btnUploadGuarantorProof" height="24px" />
										</hlayout>
									</row>
									<row id="row5">
										<label id="label_Remarks" value="${c:l('label_GuarantorDetailDialog_GuarantorRemarks.value')}" />
										<hlayout id="hlayout_Remarks">
											<space id="space_GuarantorProofName" spacing="2px" />
											<textbox id="remarks" maxlength="500" multiline="true" rows="4" width="300px" />
										</hlayout>
									</row>
									<row id="row6">
										<label value="Status" />
										<hlayout>
											<space spacing="2px"  />
											<hlayout spacing="0px">
												<textbox id="status" readonly="true" width="200px" />
											</hlayout>
										</hlayout>
										<label value="Worst Status" />
										<hlayout>
											<space spacing="2px"  />
											<textbox id="worstStatus" readonly="true" maxlength="100" width="200px" />
										</hlayout>
									</row>
								</rows>
							</grid>
						</groupbox>
						<groupbox id="gb_GurantorsPrimaryExposure"  style="border:0px" visible="false">
							<caption label="Current Exposure- Sole/Primary Joint" />
								<listbox id="listBoxGurantorsPrimaryExposure" fixedLayout="false" span="true" emptyMessage="${c:l('listbox.emptyMessage')}" multiple="true" vflex="true" width="99.8%" height="120px" sizedByContent="true">
											<listhead sizable="false">
												<listheader id="listheader_PrimaryGurantorId" label="Type" sort="auto" hflex="min" />
												<listheader id="listheader_PrimaryGurantorName" sort="auto" label="Reference" hflex="min" />
												<listheader id="listheader_PrimaryGurantorType" label="Start" sort="auto" hflex="min" />
												<listheader id="listheader_PrimaryGurantorPercentage" sort="auto" label="Maturity" hflex="min" />
												<listheader id="listheader_PrimaryCurrency" sort="auto" label="Currency" hflex="min" />
												<listheader id="listheader_PrimaryGurantorExposure" label="Finance Amount" sort="auto" hflex="min" />
												<listheader id="listheader_PrimaryGurantorFinance" sort="auto" label="Current Exposure" hflex="min" />
												<listheader id="listheader_PrimaryGurantorStatus" label="Overdue ?" sort="auto" hflex="min" />
												<listheader id="listheader_PrimaryGurantorWorstStatus" sort="auto" label="Days" hflex="min" />
												<listheader id="listheader_PrimaryGurantorMobileNo" sort="auto" label="DueAmount" hflex="min" />
											</listhead>
										</listbox>
						</groupbox>
						<groupbox id="gb_GurantorsSecoundaryExposure" visible="false" >
							<caption label="Current Exposure- Secondary Joint" />
								<listbox id="listBoxGurantorsSecoundaryExposure" fixedLayout="false" span="true" emptyMessage="${c:l('listbox.emptyMessage')}" multiple="true" vflex="true" width="99.8%" height="120px" sizedByContent="true">
											<listhead sizable="false">
												<listheader id="listheader_SecoundaryGurantorId" label="Type" sort="auto" hflex="min" />
												<listheader id="listheader_SecoundaryGurantorName" sort="auto" label="Reference" hflex="min" />
												<listheader id="listheader_SecoundaryGurantorType" label="Start" sort="auto" hflex="min" />
												<listheader id="listheader_SecoundaryGurantorPercentage" sort="auto" label="Maturity" hflex="min" />
												<listheader id="listheader_SecoundaryCurrency" sort="auto" label="Currency" hflex="min" />
												<listheader id="listheader_SecoundaryGurantorExposure" label="Finance Amount" sort="auto" hflex="min" />
												<listheader id="listheader_SecoundaryGurantorFinance" sort="auto" label="Current Exposure" hflex="min" />
												<listheader id="listheader_SecoundaryGurantorStatus" label="Overdue ?" sort="auto" hflex="min" />
												<listheader id="listheader_SecoundaryGurantorWorstStatus" sort="auto" label="Days" hflex="min" />
												<listheader id="listheader_SecoundaryGurantorMobileNo" sort="auto" label="DueAmount"  hflex="min" />
												<listheader id="listheader_SecoundaryGurantorPrimaryCIF" sort="auto" label="Primary CIF" hflex="min" />
											</listhead>
										</listbox>
						</groupbox>
						<groupbox id="gb_GurantorsExposure" visible="false" >
							<caption label="Current Exposure- Guarantor" />
									<listbox id="listBoxGurantorsExposure" fixedLayout="false" span="true" emptyMessage="${c:l('listbox.emptyMessage')}" multiple="true" vflex="true" width="99.8%" height="120px" sizedByContent="true">
											<listhead sizable="false">
												<listheader id="listheader_GurantorId" label="Type" sort="auto" hflex="min" />
												<listheader id="listheader_GurantorName" sort="auto" label="Reference" hflex="min" />
												<listheader id="listheader_GurantorType" label="Start" sort="auto" hflex="min" />
												<listheader id="listheader_GurantorPercentage" sort="auto" label="Maturity" hflex="min" />
												<listheader id="listheader_Currency" sort="auto" label="Currency" hflex="min" />
												<listheader id="listheader_GurantorExposure" label="Finance Amount" sort="auto" hflex="min" />
												<listheader id="listheader_GurantorFinance" sort="auto" label="Current Exposure" hflex="min" />
												<listheader id="listheader_GurantorStatus" label="Overdue ?" sort="auto" hflex="min" />
												<listheader id="listheader_GurantorWorstStatus" sort="auto" label="Days" hflex="min" />
												<listheader id="listheader_GurantorMobileNo" sort="auto" label="DueAmount" hflex="min" />
												<listheader id="listheader_GurantorPrimaryCIF" sort="auto" label="Primary CIF" hflex="min" />
											</listhead>
										</listbox>
						</groupbox>
					</groupbox>
				</center>
				<south id="south" height="0px" border="none">
					<groupbox id="gb_statusDetails" mold="3d" width="100%">
						<grid sclass="GridLayoutNoBorder" fixedLayout="true" style="border:0px; padding:0px;">
							<columns>
								<column width="150px" />
								<column />
								<column width="150px" />
								<column />
							</columns>
							<rows>
								<row id="statusRow">
									<label id="label_RecordStatus" value="${c:l('label_RecordStatus')}" sclass="status" />
									<label id="recordStatus" />
									<label id="label_RecordType" value="${c:l('label_RecordType')}" sclass="status" />
									<label id="recordType" />
								</row>
								<row spans="4">
									<groupbox id="groupboxWf" width="97%">
										<caption label="${c:l('Action')}" style="font-weight:bold" />
										<hlayout spacing="0px">
											<radiogroup id="userAction" name="action" />
										</hlayout>
									</groupbox>
								</row>
							</rows>
						</grid>
					</groupbox>
				</south>
			</borderlayout>
		</window>
	</window>
</zk>
