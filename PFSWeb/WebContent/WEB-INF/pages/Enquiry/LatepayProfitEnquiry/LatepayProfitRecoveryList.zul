<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window>
		<window id="window_LatepayProfitRecoveryList"
			apply="${latepayProfitRecoveryListCtrl}" border="none"
			 width="100%" height="100%" sizable="false">
					<div id="div_LatepayProfitRecoveryList"
						sclass="z-toolbar" style="padding:0px" height="28px">
						<hbox pack="stretch"
							sclass="hboxRemoveWhiteStrips" width="100%"
							widths="30%,40%,30%">

							<!-- COMMON BUTTONS -->
							<label value="" />

							<!-- SEARCH AREA -->
							<toolbar align="center"
								sclass="toolbar-center">
								<button
									id="button_LatepayProfitRecoveryList_LatepayProfitRecoverySearchDialog"
									 label="${c:l('label_Filter')}"
									autodisable="button_LatepayProfitRecoveryList_LatepayProfitRecoverySearchDialog,button_LatepayProfitRecoveryList_PrintList,button_LatepayProfitRecoveryList_NewLatepayProfitRecovery,btnRefresh,btnHelp"
									tooltiptext="${c:l('button_LatepayProfitRecoveryList_LatepayProfitRecoverySearchDialog.tooltiptext')}"  sclass="z-toolbarbutton" />
								<button
									id="button_LatepayProfitRecoveryList_PrintList" 
									label="${c:l('label_Print')}"
									autodisable="button_LatepayProfitRecoveryList_LatepayProfitRecoverySearchDialog,button_LatepayProfitRecoveryList_PrintList,button_LatepayProfitRecoveryList_NewLatepayProfitRecovery,btnRefresh,btnHelp"
									tooltiptext="${c:l('button_LatepayProfitRecoveryList_PrintList.tooltiptext')}" sclass="z-toolbarbutton" />
							</toolbar>

							<!-- CRUD BUTTONS -->
							<toolbar align="end"
								sclass="toolbar-end">
								<button id="btnRefresh" 
									label="${c:l('label_Refresh')}"
									autodisable="button_LatepayProfitRecoveryList_LatepayProfitRecoverySearchDialog,button_LatepayProfitRecoveryList_PrintList,button_LatepayProfitRecoveryList_NewLatepayProfitRecovery,btnRefresh,btnHelp"
									tooltiptext="${c:l('btnRefresh.tooltiptext')}" sclass="z-toolbarbutton" />
								<button id="btnHelp" visible="false" 
									label="${c:l('label_Help')}"
									autodisable="button_LatepayProfitRecoveryList_LatepayProfitRecoverySearchDialog,button_LatepayProfitRecoveryList_PrintList,button_LatepayProfitRecoveryList_NewLatepayProfitRecovery,btnRefresh,btnHelp"
									tooltiptext="${c:l('btnHelp.tooltiptext')}" sclass="z-toolbarbutton" />
							</toolbar>
						</hbox>
					</div>
			<borderlayout id="borderLayout_LatepayProfitRecoveryList">
				<north border="none" visible="false">
					<textbox id="recoveryCode" value="Y"
							visible="false" />
				</north>
				<center border="none" >
				<groupbox>
					<listbox id="listBoxLatepayProfitRecovery"
						emptyMessage="${c:l('listbox.emptyMessage')}" fixedLayout="true"
						  sizedByContent="true" span="true"
						tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
						height="100%" multiple="false">
						<listhead sizable="true">
							<listheader id="listheader_FinODDate"
								label="${c:l('listheader_FinOdDate.label')}" sort="false"
								hflex="true" />
							<listheader id="listheader_FinODDays"
								label="${c:l('listheader_FinODDays.label')}" sort="false"
								hflex="true" />
							<listheader id="listheader_FinODPri"
								label="${c:l('listheader_FinODPri.label')}" sort="false"
								hflex="true" />
							<listheader id="listheader_FinODPft"
								label="${c:l('listheader_FinODPft.label')}" sort="false"
								hflex="true" />
							<listheader id="listheader_FinODTot"
								label="${c:l('listheader_FinODTot.label')}" sort="false"
								hflex="true" />
							<listheader id="listheader_PenaltyAmtPerc"
								label="${c:l('listheader_PenaltyAmtPerc.label')}" sort="false"
								hflex="true" />
							<listheader id="listheader_FinODCPenalty"
								label="${c:l('listheader_FinODProfit.label')}" sort="false"
								hflex="true" />
							<listheader id="listheader_FinODCWaived" visible="false"
								label="${c:l('listheader_FinODCWaived.label')}" sort="false"
								hflex="true" />
							<listheader
								id="listheader_FinODCPenaltyPaid"
								label="${c:l('listheader_FinODProfitPaid.label')}"
								sort="false" hflex="true" />
							<listheader id="listheader_FinODCPenaltyBal"
								label="${c:l('listheader_FinODProfitBal.label')}" sort="false"
								hflex="true" />
							<listheader
								id="listheader_FinODCRecoverySts"
								label="${c:l('listheader_FinODCRecoverySts.label')}"
								sort="false" hflex="true" />
						</listhead>
					</listbox>
				</groupbox>
				</center>
			</borderlayout>
		</window>
	</window>
</zk>