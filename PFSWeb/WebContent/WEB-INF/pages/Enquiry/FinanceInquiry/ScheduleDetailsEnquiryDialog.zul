<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window>
		<window id="window_ScheduleEnquiryDialog" border="none"
			apply="${scheduleEnquiryDialogCtrl}" width="100%" height="100%"
			style="padding-top:10px" sizable="false">

			<borderlayout id="borderlayoutScheduleEnquiry">
				<center border="none" id="divCenter">

					<tabbox width="99%" height="100%"
						id="tabBoxIndexCenter">
						<tabs id="tabsIndexCenter">
							<tab id="financeTypeDetailsTab" 
								label="Schedule Details" />
							<tab id="repayGraphTab" visible="false"
								label="GraphReport" />
						</tabs>
						<tabpanels id="tabpanelsBoxIndexCenter">
							<tabpanel id="tabpanel_basicDetails"
								style="overflow:auto">
								<listbox id="listBoxSchedule"
									emptyMessage="${c:l('listbox.emptyMessage')}" vflex="true"
									width="99%" multiple="false">
									<listhead sizable="false"
										style="padding:0px;position:relative;">
										<listheader width="1%" />
										<listheader
											id="listheader_ScheduleDetailDialog_Date" hflex="min"
											value="" />
										<listheader
											id="listheader_ScheduleDetailDialog_ScheduleEvent"
											hflex="min" />
										<listheader
											id="listheader_ScheduleDetailDialog_CalProfit" hflex="min" />
										<listheader
											id="listheader_ScheduleDetailDialog_SchProfit" hflex="min" />
										<listheader
											id="listheader_ScheduleDetailDialog_Principal" hflex="min" />
										<listheader
											id="listheader_ScheduleDetailDialog_Total" hflex="min" />
										<listheader
											id="listheader_ScheduleDetailDialog_ScheduleEndBal"
											hflex="min" />
										<listheader
											id="listHeader_cashFlowEffect"
											label="${c:l('listheader_CashFlowEffect.label')}"
											hflex="min" />
										<listheader
											id="listHeader_vSProfit"
											label="${c:l('listheader_VsProfit.label')}" hflex="min" />
										<listheader
											id="listHeader_orgPrincipalDue"
											label="${c:l('listheader_OrgPrincipalDue.label')}"
											hflex="min" />
									</listhead>
								</listbox>
							</tabpanel>
							<tabpanel id="tabpanel_graph"
								style="overflow:auto">
							</tabpanel>
						</tabpanels>
					</tabbox>

				</center>
			</borderlayout>

		</window>
	</window>
</zk>
