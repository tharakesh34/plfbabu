<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="window_CustomerEmploymentDetailList"
		apply="${customerEmploymentDetailListCtrl}" border="none"
		width="100%">

		<div sclass="z-toolbar" style="padding:0px" height="28px">
			<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
				width="100%" widths="30%,40%,30%">

				<!-- COMMON BUTTONS -->
				<label value="" />

				<!-- SEARCH AREA -->
				<toolbar align="center" style=" border-style: none;">
					<button
						id="button_CustomerEmploymentDetailList_CustomerEmploymentDetailSearchDialog"
						 label="${c:l('label_Filter')}"
						autodisable="button_CustomerEmploymentDetailList_CustomerEmploymentDetailSearchDialog,button_CustomerEmploymentDetailList_PrintList,
											button_CustomerEmploymentDetailList_NewCustomerEmploymentDetail, btnRefresh, btnHelp"
						tooltiptext="${c:l('button_CustomerEmploymentDetailList_CustomerEmploymentDetailSearchDialog.tooltiptext')}" />
					<button
						id="button_CustomerEmploymentDetailList_PrintList" 
						label="${c:l('label_Print')}"
						autodisable="button_CustomerEmploymentDetailList_CustomerEmploymentDetailSearchDialog,button_CustomerEmploymentDetailList_PrintList,
											button_CustomerEmploymentDetailList_NewCustomerEmploymentDetail, btnRefresh, btnHelp"
						tooltiptext="${c:l('button_CustomerEmploymentDetailList_PrintList.tooltiptext')}" />
				</toolbar>

				<!-- CRUD BUTTONS -->
				<toolbar align="end" style=" border-style: none;">
					<button
						id="button_CustomerEmploymentDetailList_NewCustomerEmploymentDetail"
						 label="${c:l('label_New')}"
						autodisable="button_CustomerEmploymentDetailList_CustomerEmploymentDetailSearchDialog,button_CustomerEmploymentDetailList_PrintList,
											button_CustomerEmploymentDetailList_NewCustomerEmploymentDetail, btnRefresh, btnHelp"
						tooltiptext="${c:l('button_CustomerEmploymentDetailList_NewCustomerEmploymentDetail.tooltiptext')}" />

					<button id="btnRefresh" 
						label="${c:l('label_Refresh')}"
						autodisable="button_CustomerEmploymentDetailList_CustomerEmploymentDetailSearchDialog,button_CustomerEmploymentDetailList_PrintList,
											button_CustomerEmploymentDetailList_NewCustomerEmploymentDetail, btnRefresh, btnHelp"
						tooltiptext="${c:l('btnRefresh.tooltiptext')}" />
					<button id="btnHelp" 
						label="${c:l('label_Help')}"
						autodisable="button_CustomerEmploymentDetailList_CustomerEmploymentDetailSearchDialog,button_CustomerEmploymentDetailList_PrintList,
											button_CustomerEmploymentDetailList_NewCustomerEmploymentDetail, btnRefresh, btnHelp"
						tooltiptext="${c:l('btnHelp.tooltiptext')}" />
				</toolbar>
			</hbox>
		</div>

		<borderlayout id="borderLayout_CustomerEmploymentDetailList">
			<north border="none" />
			<center border="none">
				<div>
					<grid id="searchGrid" sclass="GridLayoutNoBorder"
						fixedLayout="true"
						style="border:0px; padding-left:5px; padding-right:5px;">
						<columns>
							<column width="17%" />
							<column width="5%" />
							<column width="28%" />
							<column width="17%" />
							<column width="5%" />
							<column width="28%" />
						</columns>
						<rows>
							<row>
								<label
									id="label_CustomerEmploymentDetailSearch_CustCIF"
									value="${c:l('label_CustomerEmploymentDetailSearch_CustCIF.value')}" />
								<listbox id="sortOperator_custCIF"
									width="43px" mold="select" rows="1" />
								<textbox id="custCIF" width="150px"
									maxlength="19" />
								<label
									id="label_CustomerEmploymentDetailSearch_CustEmpName"
									value="${c:l('label_CustomerEmploymentDetailSearch_CustEmpName.value')}" />
								<listbox id="sortOperator_custEmpName"
									width="43px" mold="select" rows="1" />
								<textbox id="custEmpName" width="150px"
									maxlength="50" />
							</row>
							<row>
								<label
									id="label_CustomerEmploymentDetailSearch_CustEmpDesg"
									value="${c:l('label_CustomerEmploymentDetailSearch_CustEmpDesg.value')}" />
								<listbox id="sortOperator_custEmpDesg"
									width="43px" mold="select" rows="1" />
								<textbox id="custEmpDesg" width="150px"
									maxlength="8" />
								<label
									id="label_CustomerEmploymentDetailSearch_CustEmpDept"
									value="${c:l('label_CustomerEmploymentDetailSearch_CustEmpDept.value')}" />
								<listbox id="sortOperator_custEmpDept"
									width="43px" mold="select" rows="1" />
								<textbox id="custEmpDept" width="150px"
									maxlength="8" />
							</row>
							<row>
								<label
									id="label_CustomerEmploymentDetailSearch_CustEmpID"
									value="${c:l('label_CustomerEmploymentDetailSearch_CustEmpID.value')}" />
								<listbox id="sortOperator_custEmpID"
									width="43px" mold="select" rows="1" />
								<textbox id="custEmpID" width="150px"
									maxlength="50" />
							</row>
							<row>
								<label
									id="label_CustomerEmploymentDetailSearch_RecordStatus"
									value="${c:l('label_CustomerEmploymentDetailSearch_RecordStatus.value')}" />
								<listbox id="sortOperator_recordStatus"
									width="43px" mold="select" rows="1" />
								<textbox id="recordStatus" width="150px"
									maxlength="50" />
								<label
									id="label_CustomerEmploymentDetailSearch_RecordType"
									value="${c:l('label_CustomerEmploymentDetailSearch_RecordType.value')}" />
								<listbox id="sortOperator_recordType"
									width="43px" mold="select" rows="1" />
								<listbox id="recordType" width="150px"
									mold="select" />
							</row>
						</rows>
					</grid>
					<listbox id="listBoxCustomerEmploymentDetail"
						span="true" fixedLayout="true" vflex="true"
						tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
						height="100%" multiple="false"
						emptyMessage="${c:l('listbox.emptyMessage')}">
						<listhead sizable="true">
							<listheader id="listheader_CustEmpCIF"
								label="${c:l('listheader_CustEmpCIF.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_CustEmpName"
								label="${c:l('listheader_CustEmpName.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_CustEmpDesg"
								label="${c:l('listheader_CustEmpDesg.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_CustEmpDept"
								label="${c:l('listheader_CustEmpDept.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_CustEmpID"
								label="${c:l('listheader_CustEmpID.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_RecordStatus"
								label="${c:l('listheader_RecordStatus.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_RecordType"
								label="${c:l('listheader_RecordType.label')}" sort="auto"
								hflex="min" />
						</listhead>
					</listbox>
				</div>
			</center>
			<south border="none">
				<paging id="pagingCustomerEmploymentDetailList" />
			</south>

		</borderlayout>
	</window>
</zk>