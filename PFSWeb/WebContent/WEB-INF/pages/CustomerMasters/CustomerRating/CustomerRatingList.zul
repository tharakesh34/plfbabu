<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window id="window_CustomerRatingList"
		apply="${customerRatingListCtrl}" border="none" width="100%">

		<div sclass="z-toolbar" style="padding:0" height="28px">
			<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
				width="100%" widths="30%,40%,30%">
				<!-- COMMON BUTTONS -->
				<label value="" />
				<!-- SEARCH AREA -->
				<toolbar align="center" style="border-style: none;">
					<button
						id="button_CustomerRatingList_CustomerRatingSearchDialog"
						height="24px" label = "${c:l('label_Filter')}"
						autodisable="button_CustomerRatingList_CustomerRatingSearchDialog,button_CustomerRatingList_PrintList,
											button_CustomerRatingList_NewCustomerRating, btnRefresh, btnHelp"
						tooltiptext="${c:l('button_CustomerRatingList_CustomerRatingSearchDialog.tooltiptext')}" />
					<button id="button_CustomerRatingList_PrintList"
						height="24px" label="${c:l('label_Print')}"
						autodisable="button_CustomerRatingList_CustomerRatingSearchDialog,button_CustomerRatingList_PrintList,
											button_CustomerRatingList_NewCustomerRating, btnRefresh, btnHelp"
						tooltiptext="${c:l('button_CustomerRatingList_PrintList.tooltiptext')}" />
				</toolbar>
				<!-- CRUD BUTTONS -->
				<toolbar align="end" style="border-style: none;">
					<button
						id="button_CustomerRatingList_NewCustomerRating" height="24px"
						label="${c:l('label_New')}"
						autodisable="button_CustomerRatingList_CustomerRatingSearchDialog,button_CustomerRatingList_PrintList,
											button_CustomerRatingList_NewCustomerRating, btnRefresh, btnHelp"
						tooltiptext="${c:l('button_CustomerRatingList_NewCustomerRating.tooltiptext')}" />
					<button id="btnRefresh" height="24px"
						label="${c:l('label_Refresh')}"
						autodisable="button_CustomerRatingList_CustomerRatingSearchDialog,button_CustomerRatingList_PrintList,
											button_CustomerRatingList_NewCustomerRating, btnRefresh, btnHelp"
						tooltiptext="${c:l('btnRefresh.tooltiptext')}" />
					<button id="btnHelp" height="24px"
						label="${c:l('label_Help')}"
						autodisable="button_CustomerRatingList_CustomerRatingSearchDialog,button_CustomerRatingList_PrintList,
											button_CustomerRatingList_NewCustomerRating, btnRefresh, btnHelp"
						tooltiptext="${c:l('btnHelp.tooltiptext')}" />
				</toolbar>
			</hbox>
		</div>
		<borderlayout id="borderLayout_CustomerRatingList">
			<north border="none" />

			<center border="none">

				<div>
					<grid id="searchGrid" sclass="GridLayoutNoBorder"
						fixedLayout="true"
						style="border:0px; padding-left:5px; padding-right:5px;">
						<columns>
							<column width="17%" />
							<column width="5%" />
							<column width="28%" />
							<column width="17%" />
							<column width="5%" />
							<column width="28%" />
						</columns>
						<rows>
							<row>
								<label
									id="label_CustomerRatingSearch_CustCIF"
									value="${c:l('label_CustomerRatingSearch_CustCIF.value')}" />
								<listbox id="sortOperator_custCIF"
									width="43px" mold="select" rows="1" />
								<textbox id="custCIF" width="150px"
									maxlength="19" />
								<label
									id="label_CustomerRatingSearch_CustRatingType"
									value="${c:l('label_CustomerRatingSearch_CustRatingType.value')}" />
								<listbox
									id="sortOperator_custRatingType" width="43px" mold="select"
									rows="1" />
								<textbox id="custRatingType"
									width="150px" maxlength="8" />
							</row>
							<row>
								<label
									id="label_CustomerRatingSearch_CustRatingCode"
									value="${c:l('label_CustomerRatingSearch_CustRatingCode.value')}" />
								<listbox
									id="sortOperator_custRatingCode" width="43px" mold="select"
									rows="1" />
								<textbox id="custRatingCode"
									width="150px" maxlength="8" />

								<label
									id="label_CustomerRatingSearch_CustRating"
									value="${c:l('label_CustomerRatingSearch_CustRating.value')}" />
								<listbox id="sortOperator_custRating"
									width="43px" mold="select" rows="1" />
								<textbox id="custRating" width="150px"
									maxlength="50" />
							</row>
							<row>
								<label
									id="label_CustomerRatingSearch_RecordStatus"
									value="${c:l('label_CustomerRatingSearch_RecordStatus.value')}" />
								<listbox id="sortOperator_recordStatus"
									width="43px" mold="select" rows="1" />
								<textbox id="recordStatus" width="150px"
									maxlength="50" />

								<label
									id="label_CustomerRatingSearch_RecordType"
									value="${c:l('label_CustomerRatingSearch_RecordType.value')}" />
								<listbox id="sortOperator_recordType"
									width="43px" mold="select" rows="1" />
								<listbox id="recordType" width="150px"
									mold="select" />
							</row>
						</rows>
					</grid>
					<listbox id="listBoxCustomerRating" vflex="true"
					    emptyMessage="${c:l('listbox.emptyMessage')}"
						fixedLayout="true" tooltiptext="${c:l('listbox.tooltiptext')}"
						width="100%" span="true" height="100%" multiple="false">
						<listhead sizable="true">
							<listheader id="listheader_CustCIF"
								label="${c:l('listheader_CustCIF.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_CustRatingType"
								label="${c:l('listheader_CustRatingType.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_CustRatingCode"
								label="${c:l('listheader_CustRatingCode.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_CustRating"
								label="${c:l('listheader_CustRating.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_RecordStatus"
								label="${c:l('listheader_RecordStatus.label')}" sort="auto"
								hflex="min" />
							<listheader id="listheader_RecordType"
								label="${c:l('listheader_RecordType.label')}" sort="auto"
								hflex="min" />
						</listhead>
					</listbox>
				</div>
			</center>
			<south border="none">
				<paging id="pagingCustomerRatingList" />
			</south>

		</borderlayout>
	</window>
</zk>