	<queryString>
		<![CDATA[Select InFlows,ISNULL([DAYS_0_8],0)[DAYS_0_8],ISNULL([MONTH_8_1],0)[MONTH_8_1],ISNULL([MONTH_1_3],0)[MONTH_1_3],
		 ISNULL([MONTH_3_6],0)[MONTH_3_6] FROM(
	Select InFlows,SUM(UnpaidTotalAmt)UnpaidTotalAmt,'DAYS_0_8' Clmn from RPT_ABGInF_AnylLCCYSlfFin_View
	Where NoOfDays>=0 and NoOfDays<=7 Group By InFlows
	UNION
	Select InFlows,SUM(UnpaidTotalAmt),'MONTH_8_1' from RPT_ABGInF_AnylLCCYSlfFin_View
	Where NoOfDays>7 and NoOfDays<=30 Group By InFlows
	UNION
	Select InFlows,SUM(UnpaidTotalAmt),'MONTH_1_3' from RPT_ABGInF_AnylLCCYSlfFin_View
	Where NoOfDays>30 and NoOfDays<=90 Group By InFlows
	UNION
	Select InFlows,SUM(UnpaidTotalAmt),'MONTH_3_6' from RPT_ABGInF_AnylLCCYSlfFin_View
	Where NoOfDays>90 and NoOfDays<=180 Group By InFlows )AS SourceTable
	Pivot (
	SUM(UnpaidTotalAmt)
	FOR Clmn IN ([DAYS_0_8],[MONTH_8_1],[MONTH_1_3],[MONTH_3_6])
	) AS PivotTable  $P!{whereCondition} Order By InFlows]]>
	</queryString>
