	<queryString>
		<![CDATA[SELECT  T1.FinType,T2.FinTypeDesc,FinDivision,DivisionCodeDesc,COUNT( DISTINCT T1.FinReference)NoOfFinances,T1.FinisActive,
			SUM(dbo.UDF_ConvertCurrency(PrincipalSchd+DefPrincipal,T1.FinCCY,'BHD')) TotalPrincipal,
			SUM(dbo.UDF_ConvertCurrency(ProfitSchd+DefProfit,T1.FinCCY,'BHD')) TotalProfit,
			SUM(dbo.UDF_ConvertCurrency((ProfitSchd - SchdPftPaid + DefProfit-DefSchdpftPaid),T1.FinCCY,'BHD')) ProfitSchd,
			SUM(dbo.UDF_ConvertCurrency((PrincipalSchd - SchdPriPaid + DefPrincipal-DefSchdPriPaid),T1.FinCCY,'BHD')) PrincipalSchd
	FROM    FinPftDetails T1  INNER JOIN
			FinScheduleDetails T3  ON T3.FinReference =T1.FinReference INNER JOIN
		    RMTFinanceTypes T2  ON T2.FinType=T1.FinType Inner Join
			SMTDivisionDetail T7  ON DivisionCode=FinDivision
$P!{whereCondition}
	GROUP BY T1.FinType,T2.FinTypeDesc,FinDivision,DivisionCodeDesc,T1.FinisActive Order By FinDivision,T1.FinType]]>
