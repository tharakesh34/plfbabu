	<queryString>
		<![CDATA[Select T.*,T3.FinTypeDesc,(CASE WHEN T.FinDivision='RETAIL' THEN 1 WHEN T.FinDivision='COM' THEN 2 WHEN T.FinDivision='IBD' THEN 3
		  WHEN T.FinDivision='TREASURY' THEN 4 END)FinDivOrder,DivisionCodeDesc from (
	Select CoalESce(T.FinDivision,T1.FinDivision)FinDivision,CoalESce(T.FinType,T1.FinType)FinType,CoalESce(T.Grade,T1.Grade)Grade,
	CoalESce(T.Classification,T1.Classification)Classification,CoalESce(T.DaysPastDue , T1.DaysPastDue)DaysPastDue,
	T.CurrentDate BM_CurrentDate,COALESCE(T.NoOfAccounts,0) BM_NoOfAccounts,COALESCE(T.AmountBHD,0) BM_AmountBHD,
	COALESCE(T.AmountUSD,0) BM_AmountUSD,T1.CurrentDate CD_CurrentDate,COALESCE(T1.NoOfAccounts,0) CD_NoOfAccounts,
	COALESCE(t1.AmountBHD,0) CD_AmountBHD,COALESCE(t1.AmountUSD,0) CD_AmountUSD  from (
	Select CurrentDate,FinDivision,FinType,Grade,Classification,DaysPastDue,SUM(NoOfAccounts)NoOfAccounts,
	SUM(AmountBHD)AmountBHD,SUM(AmountUSD)AmountUSD from FinODDetails_DailyDownload
	Group By CurrentDate,FinDivision,FinType,Grade,Classification,DaysPastDue)T FULL OUTER JOIN
	(Select CurrentDate,FinDivision,FinType,Grade,Classification,DaysPastDue,SUM(NoOfAccounts)NoOfAccounts,SUM(AmountBHD)AmountBHD,SUM(AmountUSD)AmountUSD from FinODDetails_DailyDownload
	Group By CurrentDate,FinDivision,FinType,Grade,Classification,DaysPastDue)T1 ON T.FinDivision=T1.FinDivision and T.FinType=T1.FinType
	and T.Grade =T1.Grade and T.Classification=T1.Classification and T.DaysPastDue = T1.DaysPastDue
	AND T.CurrentDate=$P!{fromDate} and T1.CurrentDate=$P!{toDate}
	wherE (T.CurrentDate=$P!{fromDate} OR T1.CurrentDate=$P!{toDate}))T
	Join SMTDivisionDetail ON DivisionCode=FinDivision INNER JOIN
	RMTFinanceTypes T3 ON T.FinType=T3.FinType
	$P!{whereCondition}
	order by T.FinDivision,T.FinType,Grade,Classification,DaysPastDue]]>
	</queryString>
