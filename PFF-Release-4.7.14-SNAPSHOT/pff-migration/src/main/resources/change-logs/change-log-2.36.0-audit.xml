		</column>
		<column name="AuditSeq" type="int">
		<constraints  nullable="false" />
		</column>
		<column name="AuditImage" type="varchar(2)">
		<constraints  nullable="false" />
		</column>
	</addColumn>
</changeSet>
</databaseChangeLog>
