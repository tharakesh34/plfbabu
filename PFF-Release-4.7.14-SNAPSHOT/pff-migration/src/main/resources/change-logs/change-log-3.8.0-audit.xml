 	<changeSet id="01" author="SreeRavali">
		<dropColumn columnName="RestructureReference" tableName="ADTFinancemain" />
	</changeSet>
	<changeSet id="02" author="SreeRavali">
		<addColumn tableName="ADTFinanceMain">
			<column name="OldFinReference" type="varchar2(20)" />
		</addColumn>
	</changeSet>
	<changeSet id="03" author="Surekha">
