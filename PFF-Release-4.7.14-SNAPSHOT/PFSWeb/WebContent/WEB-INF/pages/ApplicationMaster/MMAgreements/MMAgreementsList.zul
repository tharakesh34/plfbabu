<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">
	<window>
		<window id="window_MMAgreementsList"
			apply="${mMAgreementsListCtrl}" border="none" width="100%">

			<div sclass="z-toolbar" style="padding:0px" height="28px">
				<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
					width="100%" widths="30%,40%,30%">

					<!-- COMMON BUTTONS -->
					<label value="" />

					<!-- SEARCH AREA -->
					<toolbar align="center" style="toolbar-center">
						<button
							id="button_MMAgreementsList_MMAgreementsSearch" height="24px"
							label="${c:l('label_Filter')}" sclass="z-toolbarbutton"
							autodisable="button_MMAgreementsList_MMAgreementsSearch,print,button_MMAgreementsList_NewMMAgreements,btnRefresh,help"
							tooltiptext="${c:l('button_MMAgreementsList_MMAgreementsSearch.tooltiptext')}" />
						<button id="print"
							height="24px" sclass="z-toolbarbutton"
							label="${c:l('label_Print')}" visible="false"
							autodisable="button_MMAgreementsList_MMAgreementsSearch,print,button_MMAgreementsList_NewMMAgreements,btnRefresh,help"
							tooltiptext="${c:l('button_MMAgreementsList_PrintList.tooltiptext')}" />
					</toolbar>

					<!-- CRUD BUTTONS -->
					<toolbar align="end" style="toolbar-end">
						<button
							id="button_MMAgreementsList_NewMMAgreements" height="24px"
							sclass="z-toolbarbutton" visible="false"
							label="${c:l('label_New')}"
							autodisable="button_MMAgreementsList_MMAgreementsSearch,print,button_MMAgreementsList_NewMMAgreements,btnRefresh,help"
							tooltiptext="${c:l('button_MMAgreementsList_NewMMAgreements.tooltiptext')}" />

						<button id="btnRefresh" height="24px"
							sclass="z-toolbarbutton" label="${c:l('label_Refresh')}"
							autodisable="button_MMAgreementsList_MMAgreementsSearch,print,button_MMAgreementsList_NewMMAgreements,btnRefresh,help"
							tooltiptext="${c:l('btnRefresh.tooltiptext')}" />
						<button id="help" visible="false" height="24px"
							label="${c:l('label_Help')}" sclass="z-toolbarbutton"
							autodisable="button_MMAgreementsList_MMAgreementsSearch,print,button_MMAgreementsList_NewMMAgreements,btnRefresh,help"
							tooltiptext="${c:l('btnHelp.tooltiptext')}" />
					</toolbar>
				</hbox>
			</div>
			<borderlayout id="borderLayout_MMAgreementsList">
				<north border="none" />
				<center border="none">
					<div>
						<grid id="searchGrid"
							sclass="GridLayoutNoBorder" fixedLayout="true"
							style="border:0px; padding-left:5px; padding-right:5px;">
							<columns>
								<column width="15%" />
								<column width="5%" />
								<column width="30%" />
								<column width="15%" />
								<column width="5%" />
								<column width="30%" />
							</columns>
							<rows>
								<row>
									<label
										id="label_MMAgreementsList_MMAReference"
										value="${c:l('label_MMAgreementsList_MMAReference.value')}" />
									<listbox
										id="sortOperator_MMAReferennce" width="43px" mold="select"
										rows="1" />
									<textbox id="mMAReferennce"
										width="200px" maxlength="100" />
									<label
										id="label_MMAgreementsList_CustCIF"
										value="${c:l('label_MMAgreementsList_CustCIF.value')}" />
									<listbox id="sortOperator_CustCIF"
										width="43px" mold="select" rows="1" />
									<hbox>
										<textbox id="custCIF"
											width="100px" maxlength="50" />

									</hbox>
								</row>
								<row id="row_AlwWorkflow"
									visible="false">
									<label
										id="label_MMAgreementsList_RecordStatus"
										value="${c:l('label_MMAgreementsList_RecordStatus.value')}" />
									<listbox
										id="sortOperator_RecordStatus" width="43px" mold="select"
										rows="1" />
									<textbox id="recordStatus"
										width="100px" maxlength="50" />
									<label
										id="label_MMAgreementsList_RecordType"
										value="${c:l('label_MMAgreementsList_RecordType.value')}" />
									<listbox
										id="sortOperator_RecordType" width="43px" mold="select"
										rows="1" />
									<listbox id="recordType"
										width="100px" mold="select" />
								</row>
							</rows>
						</grid>

						<listbox id="listBoxMMAgreements"
							emptyMessage="${c:l('listbox.emptyMessage')}"
							tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
							height="100%" multiple="false" sizedByContent="true"
							span="true">
							<listhead sizable="true">
								<listheader id="listheader_MMAReference"
									label="${c:l('listheader_MMAReference.label')}" sort="auto"
									hflex="min" />
								<listheader id="listheader_CustCIF"
									label="${c:l('listheader_CustCIF.label')}" sort="auto"
									hflex="min" />
								<listheader id="listheader_ContractAmt"
									label="${c:l('listheader_ContractAmt.label')}" sort="auto"
									hflex="min" />
								<listheader id="listheader_ContractDate"
									label="${c:l('listheader_ContractDate.label')}" sort="auto"
									hflex="min" />
								<listheader id="listheader_Rate"
									label="${c:l('listheader_Rate.label')}" sort="auto"
									hflex="min" />
								<listheader id="listheader_RecordStatus"
									label="${c:l('listheader_RecordStatus.label')}" sort="auto"
									hflex="min" />
								<listheader id="listheader_RecordType"
									label="${c:l('listheader_RecordType.label')}" sort="auto"
									hflex="min" />
							</listhead>
						</listbox>
					</div>
				</center>

				<south border="none">
					<paging id="pagingMMAgreementsList" />
				</south>

			</borderlayout>
		</window>
	</window>
</zk>