<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<style>
		.decimalToString{ border:none; background-color:white; }
		.fontBold{ font-weight:bold; }
	</style>
	<window>
		<window id="window_CommitmentDialog" border="normal"
			apply="${commitmentDialogCtrl}" width="100%" height="100%"
			sizable="false">

			<borderlayout id="borderlayoutCommitment">
				<north>
					<div sclass="z-toolbar" style="padding:0">
						<hbox pack="stretch"
							sclass="hboxRemoveWhiteStrips" width="100%"
							widths="35%,30%,35%">

							<!-- COMMON BUTTONS -->
							<toolbar align="start"
								sclass="toolbar-start">

								<button id="btnNew" visible="false"
									tooltiptext="${c:l('btnNew.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnNew_CmtRate"
									sclass="z-toolbarbutton" />
								<button id="btnEdit" visible="false"
									tooltiptext="${c:l('btnEdit.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnNew_CmtRate"
									sclass="z-toolbarbutton" />
								<button id="btnDelete" visible="false"
									tooltiptext="${c:l('btnDelete.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnNew_CmtRate"
									sclass="z-toolbarbutton" />
								<button id="btnSave" visible="false"
									tooltiptext="${c:l('btnSave.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnNew_CmtRate"
									sclass="z-toolbarbutton" />
								<button id="btnCancel" visible="false"
									tooltiptext="${c:l('btnCancel.tooltiptext')}"
									sclass="z-toolbarbutton" />
								<button id="btnNotes" visible="false"
									tooltiptext="${c:l('btnNotes.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnNew_CmtRate"
									sclass="z-toolbarbutton" />
							</toolbar>
							<toolbar align="center"
								sclass="toolbar-center">
								<label id="label_windowTitle"
									sclass="label-heading" />
							</toolbar>

							<!-- CRUD BUTTONS -->
							<toolbar align="end" sclass="toolbar-end">
								<button id="btnHelp" visible="false"
									label="${c:l('label_Help')}"
									tooltiptext="${c:l('btnHelp.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnNew_CmtRate"
									sclass="z-toolbarbutton" />
								<button id="btnClose"
									tooltiptext="${c:l('btnClose.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnNew_CmtRate"
									sclass="z-toolbarbutton" />
							</toolbar>
						</hbox>
					</div>
				</north>
				<center border="none" autoscroll="true">
					<div>
						<tabbox>
							<tabs id="tabsIndexCenter">
								<tab id="tab_CommitmentDetails"
									label="${c:l('tab_CommitmentDetails.label')}">
								</tab>
								<tab id="tab_CommitmentMovementDetails"
									label="${c:l('tab_CommitmentMovementDetails.label')}">
								</tab>
								<tab id="tab_CommitmentPostingDetails"
									label="${c:l('tab_CommitmentPostingDetails.label')}">
								</tab>
							</tabs>
							<tabpanels id="tabpanelsBoxIndexCenter">
								<tabpanel>
									<groupbox id="gb_basicDetails"
										mold="3d">
										<grid
											sclass="GridLayoutNoBorder" fixedLayout="true"
											style="border:0px">
											<columns>
												<column width="15%" />
												<column width="35%" />
												<column width="15%" />
												<column width="35%" />
											</columns>
											<rows>
												<row>
													<label
														id="label_CmtTitle"
														value="${c:l('label_CommitmentDialog_CmtTitle.value')}" />
													<hlayout
														id="hlayout_CmtTitle" spacing="2px">
														<space
															id="space_CmtTitle" spacing="2px" />
														<textbox
															id="cmtTitle" maxlength="50" width="170px" />
													</hlayout>
												</row>
												<row id="row1">
													<label
														id="label_CustCIF"
														value="${c:l('label_CommitmentDialog_CustCIF.value')}" />
													<hlayout 
														id="hlayout_CustCIF" spacing="2px">
														<space
															id="space_CustCIF" spacing="2px" />
														<uppercasebox
															id="custCIF" width="122px" />
														<button
															id="btnSearchCustCIF"
															tooltiptext="${c:l('label_SelectCommitmentDialog_CustCIF.tooltiptext')}"
															image="/images/icons/LOVSearch.png" />
														<button
															visible="false" id="viewCustInfo"
															image="/images/icons/more.png"
															tooltiptext="${c:l('tooltip_Custinfo')}" />
														<label
															id="custName"
															style="margin-left:10px;display:inline-block;padding-top:3px;" />
													</hlayout>
													<label
														id="label_CmtBranch"
														value="${c:l('label_CommitmentDialog_CmtBranch.value')}" />
													<extendedcombobox
														id="cmtBranch" />
												</row>
												<row id="row0">
													<label
														id="label_CmtReference"
														value="${c:l('label_CommitmentDialog_CmtReference.value')}" />
													<hlayout
														id="hlayout_CmtReference" spacing="2px">
														<space
															id="space_CmtReference" spacing="2px" />
														<uppercasebox
															id="cmtReference" maxlength="20" width="170px" />
													</hlayout>
													<label
														id="label_LimitLine"
														value="${c:l('label_CommitmentDialog_LimitLine.value')}" />
													<extendedcombobox
														id="limitLine" />

												</row>
												<row visible="false">
													<label
														id="label_FacilityRef" visible="false"
														value="${c:l('label_CommitmentDialog_FacilityRef.value')}" />
													<extendedcombobox
														id="facilityRef" />
												</row>
												<row id="row2">
													<label
														id="label_CmtCcy"
														value="${c:l('label_CommitmentDialog_CmtCcy.value')}" />
													<extendedcombobox
														id="cmtCcy" />
													<label
														id="label_CmtAmount"
														value="${c:l('label_CommitmentDialog_CmtAmount.value')}" />
													<currencyBox
														id="cmtAmount" />
												</row>
												<row id="row6">
													<label
														id="label_CmtPromisedDate"
														value="${c:l('label_CommitmentDialog_CmtPromisedDate.value')}" />
													<hlayout
														id="hlayout_CmtPromisedDate" spacing="2px">
														<space
															id="space_CmtPromisedDate" spacing="2px" />
														<datebox
															id="cmtPromisedDate" width="120px" />
													</hlayout>
													<label
														id="label_CmtStartDate"
														value="${c:l('label_CommitmentDialog_CmtStartDate.value')}" />
													<hlayout
														id="hlayout_CmtStartDate" spacing="2px">
														<space
															id="space_CmtStartDate" spacing="2px" />
														<datebox
															id="cmtStartDate" width="120px" />
														<datebox
															id="cmtEndDate" width="120px" />
													</hlayout>
												</row>
												<row id="row7">
													<label
														id="label_CmtExpDate"
														value="${c:l('label_CommitmentDialog_CmtExpDate.value')}" />
													<hlayout
														id="hlayout_CmtExpDate" spacing="2px">
														<space
															id="space_CmtExpDate" spacing="2px" />
														<datebox
															id="cmtExpDate" width="120px" />
													</hlayout>
													<label
														id="label_CmtAvailableMonths"
														value="${c:l('label_CommitmentDialog_CmtAvailableMonths.value')}" />
													<hlayout
														id="hlayout_CmtAvailableMonths" spacing="2px">
														<space
															id="space_CmtAvailableMonths" spacing="2px" />
														<intbox
															id="cmtAvailableMonths" width="155px" />
													</hlayout>
												</row>
												<row>
													<label
														id="label_Revolving"
														value="${c:l('label_CommitmentDialog_Revolving.value')}" />
													<hlayout
														id="hlayout_Revolving" spacing="2px">
														<space
															id="space_Revolving" spacing="2px" />
														<checkbox
															id="revolving" checked="false" />
													</hlayout>
													<label
														id="label_CmtRvwDate"
														value="${c:l('label_CommitmentDialog_CmtRvwDate.value')}" />
													<hlayout
														id="hlayout_CmtRvwDate" spacing="2px">
														<space
															id="space_CmtRvwDate" spacing="2px" />
														<datebox
															id="cmtRvwDate" width="120px" />
													</hlayout>
												</row>
												<row>
													<label
														id="label_MultiBranch"
														value="${c:l('label_CommitmentDialog_MultiBranch.value')}" />
													<hlayout
														id="hlayout_MultiBranch" spacing="2px">
														<space
															id="space_MultiBranch" spacing="2px" />
														<checkbox
															id="multiBranch"/>
													</hlayout>
													<label
														id="label_SharedCmt"
														value="${c:l('label_CommitmentDialog_SharedCmt.value')}" />
													<hlayout
														id="hlayout_SharedCmt" spacing="2px">
														<space
															id="space_SharedCmt" spacing="2px" />
														<checkbox
															id="sharedCmt"/>
													</hlayout>
												</row>
												<row id="row3">
													<label
														id="label_OpenAccount"
														value="${c:l('label_CommitmentDialog_OpenAccount.value')}" />
													<hlayout
														id="hlayout_OpenAccount" spacing="2px">
														<space
															id="space_OpenAccount" spacing="2px" />
														<checkbox
															id="openAccount"/>
													</hlayout>
													<label
														id="label_CmtAccount"
														value="${c:l('label_CommitmentDialog_CmtAccount.value')}" />
													<hlayout
														id="hlayout_CmtAccount" spacing="2px">
														<accountSelectionBox
															id="cmtAccount" />
													</hlayout>
												</row>
												<row id="row4">
													<label
														id="label_CmtPftRateMin"
														value="${c:l('label_CommitmentDialog_CmtPftRateMin.value')}" />
													<hlayout
														id="hlayout_CmtPftRateMin" spacing="2px">
														<space
															id="space_CmtPftRateMin" spacing="2px" />
														<decimalbox
															id="cmtPftRateMin" width="170px" maxlength="15" />
													</hlayout>
													<label
														id="label_CmtPftRateMax"
														value="${c:l('label_CommitmentDialog_CmtPftRateMax.value')}" />
													<hlayout
														id="hlayout_CmtPftRateMax" spacing="2px">
														<space
															id="space_CmtPftRateMax" spacing="2px" />
														<decimalbox
															id="cmtPftRateMax" width="170px" maxlength="15" />
													</hlayout>
												</row>
												<row>
													<label
														id="label_CmtStopRateRange"
														value="${c:l('label_CommitmentDialog_CmtStopRateRange.value')}" />
													<hlayout
														id="hlayout_CmtStopRateRange" spacing="2px">
														<space
															id="space_CmtStopRateRange" spacing="2px" />
														<checkbox
															id="cmtStopRateRange" />
													</hlayout>
													<label
														id="label_CommitmentFlags"
														value="${c:l('label_CommitmentDialog_CommitmentFlags.value')}" />
													<hlayout
														id="hlayout_CommitmentFlags" spacing="2px">
														<space
															spacing="2px" />
														<hlayout
															spacing="0px">
															<textbox
																id="commitmentFlags" width="170px" readonly="true" />
															<button
																id="btnSearchCommitmentFlags"
																image="/images/icons/LOVSearch.png" />
														</hlayout>
													</hlayout>
												</row>
												<row id="row_Status">
													<label
														id="label_CmtActiveStatus"
														value="${c:l('label_CommitmentDialog_CmtActiveStatus.value')}" />
													<hlayout
														id="hlayout_CmtActiveStatus" spacing="2px">
														<space
															id="space_CmtActiveStatus" spacing="2px" />
														<checkbox
															id="cmtActiveStatus"/>
													</hlayout>
													<label
														id="label_CmtNonperformingStatus"
														value="${c:l('label_CommitmentDialog_CmtNonperformingStatus.value')}" />
													<hlayout
														id="hlayout_CmtNonperformingStatus" spacing="2px">
														<space
															id="space_CmtNonperformingStatus" spacing="2px" />
														<checkbox
															id="cmtNonperformingStatus"/>
													</hlayout>
												</row>
												<row id="row8">
													<label
														id="label_CmtCharges"
														value="${c:l('label_CommitmentDialog_CmtCharges.value')}" />
													<hlayout
														id="hlayout_CmtCharges" spacing="2px">
														<currencyBox
															id="cmtCharges" />
													</hlayout>
													<label
														id="label_CmtChargesAccount"
														value="${c:l('label_CommitmentDialog_CmtChargesAccount.value')}" />
													<hlayout
														id="hlayout_CmtChargesAccount" spacing="2px">
														<accountSelectionBox
															id="cmtChargesAccount" />
													</hlayout>
												</row>
												<row id="row10">
													<label
														id="label_CollateralRequired"
														value="${c:l('label_CommitmentDialog_CollateralRequired.value')}" />
													<hlayout
														id="hlayout_CollateralRequired" spacing="2px">
														<space
															id="space_CollateralRequired" spacing="2px" />
														<checkbox
															id="collateralRequired"/>
													</hlayout>
												</row>

												<row id="row5"
													visible="false">
													<label
														id="label_CmtAvailable"
														value="${c:l('label_CommitmentDialog_CmtAvailable.value')}" />
													<hlayout
														id="hlayout_CmtAvailable">
														<space
															id="space_CmtAvailable" spacing="2px" />
														<decimalbox
															id="cmtAvailable" width="162px" maxlength="25" />
													</hlayout>
													<label
														id="label_CmtUtilizedAmount"
														value="${c:l('label_CommitmentDialog_CmtUtilizedAmount.value')}" />
													<hlayout
														id="hlayout_CmtUtilizedAmount">
														<space
															id="space_CmtUtilizedAmount" spacing="2px" />
														<decimalbox
															id="cmtUtilizedAmount" width="162px" maxlength="25" />
													</hlayout>
												</row>

											</rows>
										</grid>
									</groupbox>

									<!-- Basic Details End  -->

									<groupbox mold="3d">
										<grid
											sclass="GridLayoutNoBorder" fixedLayout="true"
											style="border:0px">
											<columns>
												<column width="15%" />
												<column width="35%" />
												<column width="15%" />
												<column width="35%" />
											</columns>
											<rows>
												<row id="row12"
													spans="1,1,2">
													<label
														id="label_CmtNotes"
														value="${c:l('label_CommitmentDialog_CmtNotes.value')}" />
													<hlayout
														id="hlayout_CmtNotes">
														<space
															id="space_CmtNotes" spacing="2px" />
														<textbox
															id="cmtNotes" maxlength="600" multiline="true" rows="23"
															style="resize:none;" width="400px" />
													</hlayout>

													<vlayout>
														<!-- Commitment Review Rates -->
														<groupbox
															id="gb_CmtReviewRates">
															<div
																sclass="z-toolbar" style="padding:0">
																<hbox
																	pack="stretch" sclass="hboxRemoveWhiteStrips"
																	width="100%" height="24px" align="center">

																	<toolbar
																		align="start" sclass="toolbar-start">
																		<label
																			value="${c:l('label_CmtReviewRates')}"
																			style="color: #ff4500; font-weight: bold;" />
																	</toolbar>
																	<toolbar
																		align="end" sclass="toolbar-end">
																		<button
																			id="btnNew_CmtRate" label="${c:l('label_NewRates')}"
																			tooltiptext="${c:l('btnNew_CmtRate.tooltiptext')}" visible="false"
																			autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose,btnNew_CmtRate"
																			sclass="z-toolbarbutton" />
 																	</toolbar>
																</hbox>
															</div>
															<listbox
																id="listBoxCmtRates" span="true" sizedByContent="true"
																tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
																multiple="false"
																emptyMessage="${c:l('listbox.emptyMessage')}">
																<listhead
																	sizable="true">
																	<listheader
																		id="listheader_CmtRate_RvwFrq"
																		label="${c:l('listheader_CmtRate_RvwFrq.label')}"
																		sort="false" hflex="min" />
																	<listheader
																		id="listheader_CmtRate_RefRate"
																		label="${c:l('listheader_CmtRate_RefRate.label')}"
																		sort="false" hflex="min" />
																	<listheader
																		id="listheader_CmtRate_Margin"
																		label="${c:l('listheader_CmtRate_Margin.label')}"
																		sort="false" hflex="min" />
																	<listheader
																		id="listheader_CmtRate_ActualRate"
																		label="${c:l('listheader_CmtRate_ActualRate.label')}"
																		sort="false" hflex="min" />
																	<listheader
																		id="listheader_CmtRate_CalRate"
																		label="${c:l('listheader_CmtRate_CalRate.label')}"
																		sort="false" hflex="min" />
																</listhead>
															</listbox>
														</groupbox>

														<!-- Customer Existing Commitments-->
														<groupbox
															id="gbCommitmentSummary" visible="false">
															<caption
																label="${c:l('label_Customer_Existing_Commitments')}">
															</caption>
															<listbox
																id="commitmentSummary" sizedByContent="true" span="true"
																height="100px"
																emptyMessage="${c:l('listbox.emptyMessage')}">
																<listhead>
																	<listheader
																		label="${c:l('label_CommitmentSummary_CmtCcy')}"
																		sort="none" hflex="min" />
																	<listheader
																		label="${c:l('label_CommitmentSummary_CmtCommitments')}"
																		sort="none" hflex="min" />
																	<listheader
																		label="${c:l('label_CommitmentSummary_CmtAmount')}"
																		sort="none" hflex="min" />
																	<listheader
																		label="${c:l('label_CommitmentSummary_CmtUtilizedTotAmount')}"
																		sort="none" hflex="min" />
																	<listheader
																		label="${c:l('label_CommitmentSummary_CmtUnUtilizedAmount')}"
																		sort="none" hflex="min" />
																</listhead>
															</listbox>
														</groupbox>

														<!-- Commitment Summary -->
														<grid
															width="100%" style="background-color:#f38c60;"
															id="gridSummary" visible="false">
															<columns>
																<column />
																<column />
															</columns>
															<rows>
																<row
																	spans="2" id="rowCmtSummary">
																	<label
																		value="${c:l('label_Customer_Existing_Commitments')}"
																		sclass="fontBold" />
																</row>
																<row
																	id="rowCmtCount">
																	<label
																		id="label_CmtCommitments"
																		value="${c:l('label_CommitmentDialog_CmtCommitments.value')}"
																		sclass="fontBold" />
																	<label
																		id="cmtCommitments" sclass="fontBold" />
																</row>
																<row
																	id="rowCmtTotAmount">
																	<label
																		id="label_CmtTotAmount"
																		value="${c:l('label_CommitmentDialog_CmtAmount.value')}"
																		sclass="fontBold" />
																	<label
																		id="cmtTotAmount" sclass="fontBold" />
																</row>
																<row
																	id="rowCmtUtilized">
																	<label
																		id="label_CmtUtilizedTotAmount"
																		value="${c:l('label_CommitmentDialog_CmtUtilizedTotAmount.value')}"
																		sclass="fontBold" />
																	<label
																		id="cmtUtilizedTotAmount" sclass="fontBold" />
																</row>
																<row
																	id="rowCmtUnUtilized">
																	<label
																		id="label_CmtUnUtilizedAmount"
																		value="${c:l('label_CommitmentDialog_CmtUnUtilizedAmount.value')}"
																		sclass="fontBold" />
																	<label
																		id="cmtUnUtilizedAmount" sclass="fontBold" />
																</row>
															</rows>
														</grid>
													</vlayout>
												</row>
											</rows>
										</grid>
									</groupbox>
								</tabpanel>
								<tabpanel>
									<!-- Loan Details -->
									<groupbox>
										<caption label="Loan Details" />
