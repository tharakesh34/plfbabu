<?xml version="1.0" encoding="UTF-8" ?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">
    <style if="${execution.explorer}">
	  .iframe-style {width : 100%; height :96%;} 
	 </style>
	 <style unless="${execution.explorer}">
	  .iframe-style {width : 100%; height :100%;} 
	 </style>
	<script type="text/javascript">
		function setSave() {
			//var svg = frames[0].frames[2].ORYX.EDITOR.getCanvas().getSVGRepresentation();
			
			//alert($("#frameEditor").contents().find(".ext-gen251").length);
			
			//alert(frames[5].contentWindow);
			
			var arrFrames = parent.document.getElementsByTagName("IFRAME");
			
			for (var i = 0; i &lt; arrFrames.length; i++) {
				alert(arrFrames[i].id);
				//if (arrFrames[i].contentWindow === window) alert("yay!");
			}
		}
	</script>
	<script type="text/javascript">
	// example variable and function for cross-document demo
   
     function clearGreeting() {
   		alert("Successss");
    }

       // check for browser support
      if ( window.addEventListener ) {
    
       // message handler
       window.addEventListener('message', function (e) {
        // check message origin
        if ( e.origin === 'http://192.168.1.162:8080/activiti-app/' ) {
            var task = e.data['task']; // task received in postMessage
            switch ( task ) { // postMessage tasks
                
                case 'clear' :
                    clearGreeting();
                    break;
                
                //default:
                    
               }
        
           }
        
       }, false);
    }
	
	
	
	</script>
	
	
	<window id="window_workflowDesign" border="none"
		apply="${workflowDesignCtrl}" width="100%" height="100%">

		<div sclass="z-toolbar" style="padding:0">
			<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
				width="100%" widths="35%,30%,35%">
				<!-- COMMON BUTTONS -->
				<toolbar align="start" sclass="toolbar-start">
					<button id="btnSave" 
						label="${c:l('btnSave.label')}"
						tooltiptext="${c:l('btnSave.tooltiptext')}" 
						sclass="z-toolbarbutton" visible="false"/>
					<button id="btnCancel" 
						label="${c:l('btnCancel.label')}"
						tooltiptext="${c:l('btnCancel.tooltiptext')}" 
						sclass="z-toolbarbutton"  visible="false"/>

				</toolbar>
				<!-- SEARCH AREA -->
				<toolbar align="center" sclass="toolbar-center">
					<label value="${c:l('window_workflowDesign.title')}"
						sclass="label-heading" />
				</toolbar>
				<!-- CRUD BUTTONS -->
				<toolbar align="end" sclass="toolbar-end">
					<button id="btnClose" 
						label="${c:l('btnClose.label')}"
						tooltiptext="${c:l('btnClose.tooltiptext')}" sclass="z-toolbarbutton" />
				</toolbar>
			</hbox>
		</div>
		<iframe id="iframe" name="iframe"
			src="http://www.pennanttech.com/" sclass="iframe-style" />
	</window>
</zk>