<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window>
		<window id="window_FeeReceiptList" apply="${feeReceiptListCtrl}"
			border="none" width="100%">
			<div sclass="z-toolbar" style="padding:0px">
				<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
					width="100%" widths="35%,30%,35%">

					<!-- COMMON BUTTONS -->
					<label value="" />

					<!-- SEARCH AREA -->
					<toolbar align="center" sclass="toolbar-center">
						<button id="btnSearch"
							label="${c:l('label_Filter')}"
							autodisable="btnSearch,print,btnNew, btnRefresh, help"
							tooltiptext="${c:l('btnSearch.tooltiptext')}"
							sclass="z-toolbarbutton" />
						<button id="print" label="${c:l('label_Print')}"
							visible="false"
							autodisable="btnSearch,print,btnNew, btnRefresh, help"
							tooltiptext="${c:l('btnPrint.tooltiptext')}"
							sclass="z-toolbarbutton" />
					</toolbar>

					<!-- CRUD BUTTONS -->
					<toolbar align="end" sclass="toolbar-end">
						<button id="btnNew" visible="false"
							label="${c:l('label_New')}"
							autodisable="btnSearch,print,btnNew, btnRefresh, help"
							tooltiptext="${c:l('btnNew.tooltiptext')}"
							sclass="z-toolbarbutton" />
						<button id="btnRefresh"
							label="${c:l('label_Refresh')}"
							autodisable="btnSearch,print,btnNew, btnRefresh, help"
							tooltiptext="${c:l('btnRefresh.tooltiptext')}"
							sclass="z-toolbarbutton" />
						<button id="help" visible="false"
							label="${c:l('label_Help')}"
							autodisable="btnSearch,print,btnNew, btnRefresh, help"
							tooltiptext="${c:l('btnHelp.tooltiptext')}"
							sclass="z-toolbarbutton" />
					</toolbar>
				</hbox>
			</div>
			<borderlayout id="borderLayout_FeeReceiptList">
				<north border="none" />
				<center border="none">
					<div>
						<grid id="searchGrid"
							sclass="GridLayoutNoBorder" fixedLayout="true">
							<columns>
								<column width="130px" />
								<column width="50px" />
								<column />
								<column width="130px" />
								<column width="50px" />
								<column />
							</columns>
							<rows>
								<row>
									<label
										id="label_FeeReceiptSearch_FeeReceiptReference"
										value="${c:l('label_FeeReceiptSearch_FeeReceiptReference.value')}" />
									<listbox
										id="sortOperator_FeeReceiptReference" width="43px"
										mold="select" rows="1" />
									<uppercasebox id="receiptReference"
										width="150px" maxlength="20" />
									<label
										id="label_FeeReceiptSearch_FeeReceiptCustomer"
										value="${c:l('label_FeeReceiptSearch_FeeReceiptCustomer.value')}" />
									<listbox
										id="sortOperator_FeeReceiptCustomer" width="43px"
										mold="select" rows="1" />
									<hbox>
										<uppercasebox id="customer"
											width="150px" />
										<button id="btnSearchCustCIF"
											tooltiptext="${c:l('label_FeeReceiptSearch_btnSearchCustCIF.tooltiptext')}"
											image="/images/icons/more.png" />
									</hbox>
								</row>
								<row>
									<label
										id="label_FeeReceiptSearch_FeeReceiptFinType"
										value="${c:l('label_FeeReceiptSearch_FeeReceiptFinType.value')}" />
									<listbox
										id="sortOperator_FeeReceiptFinType" width="43px" mold="select"
										rows="1" />
									<hbox>
										<uppercasebox id="finType"
											width="150px" />
										<button id="btnSearchFinType"
											tooltiptext="${c:l('label_FeeReceiptSearch_btnSearchFinType.tooltiptext')}"
											image="/images/icons/LOVSearch.png" />
									</hbox>
									<label
										id="label_FeeReceiptSearch_FeeReceiptReceiptMode"
										value="${c:l('label_FeeReceiptSearch_FeeReceiptReceiptMode.value')}" />
									<listbox
										id="sortOperator_FeeReceiptReceiptMode" width="43px"
										mold="select" rows="1" />
									<combobox id="receiptMode"
										width="180px" maxlength="100" readonly="true" />
								</row>
								<row visible="false">
									<label
										id="label_FeeReceiptSearch_ReceiptAllocationType"
										value="${c:l('label_FeeReceiptSearch_ReceiptAllocationType.value')}" />
									<listbox
										id="sortOperator_FeeReceiptAllocationType" width="43px"
										mold="select" rows="1" />
									<combobox id="allocationType"
										width="180px" maxlength="100" readonly="true" />
									<label
										id="label_FeeReceiptSearch_FeeReceiptPurpose"
										value="${c:l('label_FeeReceiptSearch_FeeReceiptPurpose.value')}" />
									<listbox
										id="sortOperator_FeeReceiptPurpose" width="43px" mold="select"
										rows="1" />
									<combobox id="purpose" width="180px"
										maxlength="100" readonly="true" />
								</row>
								<row>
									<label
										id="label_FeeReceiptSearch_FeeReceiptFinBranch"
										value="${c:l('label_FeeReceiptSearch_FeeReceiptFinBranch.value')}" />
									<listbox
										id="sortOperator_FeeReceiptFinBranch" width="43px"
										mold="select" rows="1" />
									<hbox>
										<uppercasebox id="finBranch"
											width="150px" />
										<button id="btnSearchBranch"
											tooltiptext="${c:l('label_FeeReceiptSearch_btnSearchBranch.tooltiptext')}"
											image="/images/icons/LOVSearch.png" />
									</hbox>
								</row>
								<row>
									<label
										id="label_FeeReceiptSearch_RecordStatus"
										value="${c:l('label.RecordStatus')}" />
									<listbox
										id="sortOperator_RecordStatus" width="43px" mold="select"
										rows="1" />
									<textbox id="recordStatus"
										width="100px" maxlength="50" />
									<label
										id="label_FeeReceiptSearch_RecordType"
										value="${c:l('label.RecordType')}" />
									<listbox
										id="sortOperator_RecordType" width="43px" mold="select"
										rows="1" />
									<listbox id="recordType"
										width="100px" mold="select" />
								</row>
								<row id="workFlowFrom">
									<label
										value="${c:l('common.fetch.selectfrom')}" />
									<cell colspan="5">
										<radiogroup>
											<radio id="fromApproved"
												selected="true"
												label="${c:l('common.fetch.fromapproved')}">
											</radio>
											<radio id="fromWorkFlow"
												label="${c:l('common.fetch.fromWorkFlow')}">
											</radio>
										</radiogroup>
									</cell>
								</row>
							</rows>
						</grid>
						<listbox id="listBoxFeeReceipt"
							emptyMessage="${c:l('listbox.emptyMessage')}"
							tooltiptext="${c:l('listbox.tooltiptext')}" width="100%"
							height="100%" multiple="false" sizedByContent="true"
							span="true">
							<listhead sizable="true">
								<listheader
									id="listheader_FeeReceiptReference"
									label="${c:l('listheader_FeeReceiptReference.label')}"
									sort="auto" hflex="min" />
								<listheader
									id="listheader_FeeReceiptPurpose" visible="false"
									label="${c:l('listheader_FeeReceiptPurpose.label')}"
									sort="auto" hflex="min" />
								<listheader
									id="listheader_FeeReceiptMode"
									label="${c:l('listheader_FeeReceiptMode.label')}" sort="auto"
									hflex="min" />
								<listheader
									id="listheader_FeeReceiptAmount" align="right"
									label="${c:l('listheader_FeeReceiptAmount.label')}" sort="auto"
									hflex="min" />
								<listheader
									id="listheader_FeeReceiptAllocattionType" visible="false"
									label="${c:l('listheader_FeeReceiptAllocattionType.label')}"
									sort="auto" hflex="min" />
								<listheader
									id="listheader_FeeReceiptFinType"
									label="${c:l('listheader_FeeReceiptFinType.label')}"
									sort="auto" hflex="min" />
								<listheader
									id="listheader_FeeReceiptFinBranch"
									label="${c:l('listheader_FeeReceiptFinBranch.label')}"
									sort="auto" hflex="min" />
								<listheader
									id="listheader_FeeReceiptCusomer"
									label="${c:l('listheader_FeeReceiptCusomer.label')}"
									sort="auto" hflex="min" />
								<listheader
									id="listheader_FeeReceiptCustName" visible="false"
									label="${c:l('listheader_FeeReceiptCustName.label')}"
									sort="auto" hflex="min" />
								<listheader
									id="listheader_FeeReceiptModeSts" visible="false"
									label="${c:l('listheader_FeeReceiptModeSts.label')}"
									sort="auto" hflex="min" />
								<listheader id="listheader_RecordStatus"
									label="${c:l('label.RecordStatus')}" sort="auto" hflex="min" />
								<listheader id="listheader_RecordType"
									label="${c:l('label.RecordType')}" sort="auto" hflex="min" />
							</listhead>
						</listbox>
					</div>
				</center>
				<south border="none">
					<paging id="pagingFeeReceiptList" />
				</south>
			</borderlayout>
		</window>
	</window>
</zk>