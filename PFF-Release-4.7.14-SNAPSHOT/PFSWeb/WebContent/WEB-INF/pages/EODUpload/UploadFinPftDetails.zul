<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>
<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">
	<window id="window_UploadFinPftDetails" border="normal" apply="${uploadFinPftDetailsCtrl}" width="100%" height="100%" sizable="false">
		<timer id="timer" delay="500" repeats="true" />
		<div sclass="z-toolbar">
			<hbox pack="stretch" sclass="hboxRemoveWhiteStrips"
				width="100%" widths="35%,30%,35%">
				<!-- COMMON BUTTONS -->
				<toolbar align="start" sclass="toolbar-start">

				</toolbar>
				<!-- SEARCH AREA -->
				<toolbar align="center" sclass="toolbar-center">
					<label
						value="${c:l('window_UploadFinPftDetails.title')}"
						sclass="label-heading" />
				</toolbar>
				<!-- CRUD BUTTONS -->
				<toolbar align="end" sclass="toolbar-end">

				</toolbar>
			</hbox>
		</div>
		<borderlayout id="borderlayoutCustomerRating">
			<center border="none" autoscroll="false">
				<vlayout>
					<groupbox mold="3d">
						<grid fixedLayout="true" sclass="GridLayoutNoBorder" style="border:0px">
							<columns>
								<column width="100%" />
							</columns>
							<rows>
								<row>
									<button id="btnUpload"  tooltiptext="${c:l('label_BatchAdminCtrl_Run.tooltiptext')}" label="${c:l('label_UploadEOD_Upload.value')}" />
								</row>
							</rows>
						</grid>
						<hbox align="center">
							<exePannel id="processs" border="normal" style="padding:2px;" title="${c:l('label_uploadPftDetails')}" width="350px" imgSrc="/images/icons/send.png" />
						</hbox>
					</groupbox>
				</vlayout>
			</center>
		</borderlayout>
	</window>
</zk>
