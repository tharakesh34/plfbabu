		FROM   [RPT_ABGInF_AnylsFCCY_RIA_View]
		WHERE  NoOfDays>=0 and NoOfDays<=7 GROUP BY INFLOWS
		UNION
		SELECT INFLOWS,'MONTH_8_1'Colum, SUM(Amount)Measure
		FROM   [RPT_ABGInF_AnylsFCCY_RIA_View]
		WHERE  NoOfDays>=8 and NoOfDays<=30	GROUP BY INFLOWS
		UNION
		SELECT INFLOWS,'MONTH_1_3'Colum, SUM(Amount)Measure
		FROM   [RPT_ABGInF_AnylsFCCY_RIA_View]
		WHERE  NoOfDays>=31 and NoOfDays<=90 GROUP BY INFLOWS
		UNION
		SELECT INFLOWS,'MONTH_3_6'Colum, SUM(Amount)Measure
		FROM   [RPT_ABGInF_AnylsFCCY_RIA_View]
