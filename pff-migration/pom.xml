<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.pennanttech.pennapps.pff</groupId>
	<artifactId>pff-migration</artifactId>
	<version>2.6.0</version>
	<packaging>jar</packaging>

	<name>pff-migration</name>
	<url>http://maven.apache.org</url>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<liquibase.version>3.5.3</liquibase.version>
		<liquibase.ext.version>1.3.0</liquibase.ext.version>
	</properties>

	<repositories>
		<repository>
			<id>central</id>
			<name>Central Repository</name>
			<url>http://nexus-pennant-services/nexus/content/groups/public/</url>
		</repository>
	</repositories>

	<pluginRepositories>
		<pluginRepository>
			<id>central</id>
			<name>Central Repository</name>
			<url>http://nexus-pennant-services/nexus/content/groups/public/</url>
		</pluginRepository>
	</pluginRepositories>

	<dependencies>
		<!-- JDBC Driver -->
		<dependency>
			<groupId>com.microsoft.sqlserver</groupId>
			<artifactId>sqljdbc4</artifactId>
			<version>4.0.2206.100</version>
		</dependency>
		<dependency>
			<groupId>com.oracle</groupId>
			<artifactId>ojdbc8</artifactId>
			<version>12.2.0.1.0</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.postgresql</groupId>
			<artifactId>postgresql</artifactId>
			<version>9.2-1002-jdbc4</version>
		</dependency>

		<!-- Liquibase -->
		<dependency>
			<groupId>org.liquibase</groupId>
			<artifactId>liquibase-core</artifactId>
			<version>${liquibase.version}</version>
		</dependency>
		<dependency>
			<groupId>pennapps.liquibase.ext</groupId>
			<artifactId>pennapps-liquibase-ext</artifactId>
			<version>${liquibase.ext.version}</version>
		</dependency>
	</dependencies>

	<profiles>
		<profile>
			<id>dev</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>oracle.jdbc.driver.OracleDriver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:oracle:thin:@192.168.1.19:1521:orcl</pfs.liquibase.url>
				<pfs.liquibase.username>PLF_CORE_DEV</pfs.liquibase.username>
				<pfs.liquibase.password>pff123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>devAudit</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master-audit.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>oracle.jdbc.driver.OracleDriver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:oracle:thin:@192.168.1.19:1521:orcl</pfs.liquibase.url>
				<pfs.liquibase.username>PLF_CORE_DEV_AUDIT</pfs.liquibase.username>
				<pfs.liquibase.password>pff123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>oracledev</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>oracle.jdbc.driver.OracleDriver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:oracle:thin:@192.168.1.19:1521:orcl</pfs.liquibase.url>
				<pfs.liquibase.username>plfdev</pfs.liquibase.username>
				<pfs.liquibase.password>pff123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>oracledevaudit</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master-audit.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>oracle.jdbc.driver.OracleDriver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:oracle:thin:@192.168.1.19:1521:orcl</pfs.liquibase.url>
				<pfs.liquibase.username>plfauditdev</pfs.liquibase.username>
				<pfs.liquibase.password>pff123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>oracleqc</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>oracle.jdbc.driver.OracleDriver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:oracle:thin:@192.168.1.19:1521:orcl</pfs.liquibase.url>
				<pfs.liquibase.username>plf</pfs.liquibase.username>
				<pfs.liquibase.password>pff123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>oracleqcaudit</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master-audit.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>oracle.jdbc.driver.OracleDriver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:oracle:thin:@192.168.1.19:1521:orcl</pfs.liquibase.url>
				<pfs.liquibase.username>plfaudit</pfs.liquibase.username>
				<pfs.liquibase.password>pff123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>local</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>oracle.jdbc.driver.OracleDriver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:oracle:thin:@localhost:1521:orcl</pfs.liquibase.url>
				<pfs.liquibase.username>plfpro</pfs.liquibase.username>
				<pfs.liquibase.password>pff123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>localaudit</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master-audit.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>oracle.jdbc.driver.OracleDriver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:oracle:thin:@localhost:1521:orcl</pfs.liquibase.url>
				<pfs.liquibase.username>plfproaudit</pfs.liquibase.username>
				<pfs.liquibase.password>pff123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>postgres</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>org.postgresql.Driver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:postgresql://192.168.1.26:5432/plfedb</pfs.liquibase.url>
				<pfs.liquibase.liquibaseSchemaName>plf</pfs.liquibase.liquibaseSchemaName>
				<pfs.liquibase.defaultSchemaName>plf</pfs.liquibase.defaultSchemaName>
				<pfs.liquibase.username>postgres</pfs.liquibase.username>
				<pfs.liquibase.password>Pennant_123</pfs.liquibase.password>
			</properties>
		</profile>
		<profile>
			<id>postgresaudit</id>
			<properties>
				<pfs.liquibase.changeLogFile>change-log-master-audit.xml</pfs.liquibase.changeLogFile>
				<pfs.liquibase.driver>org.postgresql.Driver</pfs.liquibase.driver>
				<pfs.liquibase.url>jdbc:postgresql://192.168.1.26:5432/plfedb</pfs.liquibase.url>
				<pfs.liquibase.liquibaseSchemaName>plfaudit</pfs.liquibase.liquibaseSchemaName>
				<pfs.liquibase.defaultSchemaName>plfaudit</pfs.liquibase.defaultSchemaName>
				<pfs.liquibase.username>postgres</pfs.liquibase.username>
				<pfs.liquibase.password>Pennant_123</pfs.liquibase.password>
			</properties>
		</profile>
	</profiles>

	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>${jdk.version}</source>
					<target>${jdk.version}</target>
					<!-- <fork>true</fork> -->
					<meminitial>128m</meminitial>
					<maxmem>1024m</maxmem>
					<compilerArguments>
						<verbose />
						<bootclasspath>${java.home}/lib/rt.jar${path.separator}${java.home}/lib/jce.jar</bootclasspath>
					</compilerArguments>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.liquibase</groupId>
				<artifactId>liquibase-maven-plugin</artifactId>
				<version>${liquibase.version}</version>
				<configuration>
					<propertyFile>liquibase.properties</propertyFile>
					<changeLogFile>${pfs.liquibase.changeLogFile}</changeLogFile>
					<driver>${pfs.liquibase.driver}</driver>
					<url>${pfs.liquibase.url}</url>
					<changelogSchemaName>${pfs.liquibase.liquibaseSchemaName}</changelogSchemaName>
					<defaultSchemaName>${pfs.liquibase.defaultSchemaName}</defaultSchemaName>
					<username>${pfs.liquibase.username}</username>
					<password>${pfs.liquibase.password}</password>
					<promptOnNonLocalDatabase>false</promptOnNonLocalDatabase>
					<verbose>false</verbose>
				</configuration>
				<dependencies>
					<dependency>
						<groupId>pennapps.liquibase.ext</groupId>
						<artifactId>pennapps-liquibase-ext</artifactId>
						<version>${liquibase.ext.version}</version>
					</dependency>
				</dependencies>
				<executions>
					<execution>
						<phase>process-resources</phase>
						<goals>
							<goal>update</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings 
					only. It has no influence on the Maven build itself. -->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>org.liquibase</groupId>
										<artifactId>
											liquibase-maven-plugin
										</artifactId>
										<versionRange>
											[3.4.1,)
										</versionRange>
										<goals>
											<goal>update</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
</project>
