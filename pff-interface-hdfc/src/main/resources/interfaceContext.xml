<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/aop
		http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
		http://www.springframework.org/schema/tx
		http://www.springframework.org/schema/tx/spring-tx-3.0.xsd
		http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean id="smsGateway" class="com.pennanttech.pennapps.notification.sms.SmsGateway">
		<property name="url" value="${sms.gateway.url}" />
		<property name="senderId" value="${sms.gateway.sender.id}" />
		<property name="channel" value="${sms.gateway.channel}" />
		<property name="dcs" value="${sms.gateway.dcs}" />
		<property name="flashSms" value="${sms.gateway.flashsms}" />
		<property name="route" value="${sms.gateway.route}" />
	</bean>

	<bean id="otpEmailEngine" class="com.pennanttech.pennapps.notification.email.EmailEngineThread">
		<constructor-arg ref="emailEngine" />
		<constructor-arg value="0" />
		<constructor-arg name="emailService">
			<null />
		</constructor-arg>
	</bean>



	<bean id="otpService" class="com.pennapps.security.core.otp.impl.OTPServiceImpl">
		<property name="smsGateway" ref="smsGateway" />
		<property name="otpEmailEngine" ref="otpEmailEngine" />
	</bean>
	<!-- Below code is for HDFC External Presentment SI, SI INternal and ACH -->

	<bean id="externalPresentmentDAO" class="com.pennanttech.external.presentment.dao.ExtPresentmentDAOImpl">
		<property name="mainDataSource" ref="dataSource" />
		<property name="extDataSource" ref="extDataSource" />
		<property name="stagingDataSource" ref="stagingDataSource" />
	</bean>

	<bean id="extGenericDao" class="com.pennanttech.external.app.config.dao.ExtGenericDaoImpl">
		<property name="extDataSource" ref="extDataSource" />
	</bean>

	<bean id="interfaceErrorCodeUtil" class="com.pennanttech.external.app.util.InterfaceErrorCodeUtil">
		<constructor-arg ref="extGenericDao" />
	</bean>
	
	<bean id="fileInterfaceConfigUtil" class="com.pennanttech.external.app.util.FileInterfaceConfigUtil">
		<property name="extGenericDao" ref="extGenericDao" />
	</bean>
	
	<bean id="fileTransferConfigUtil" class="com.pennanttech.external.app.util.FileTransferConfigUtil">
		<property name="extGenericDao" ref="extGenericDao" />
	</bean>

	<bean id="extUcicDao" class="com.pennanttech.external.ucic.dao.ExtUcicDaoImpl">
		<property name="mainDataSource" ref="dataSource" />
	</bean>
	
	<bean id="externalDao" class="com.pennanttech.external.app.config.dao.ExternalDao">
		<property name="extDataSource" ref="dataSource" />
	</bean>

	<bean id="extExtractionDao" class="com.pennanttech.external.extractions.dao.ExtExtractionDaoImpl">
		<property name="mainDataSource" ref="dataSource" />
		<property name="extDataSource" ref="extDataSource" />
		<property name="stagingDataSource" ref="stagingDataSource" />
	</bean>

	<bean id="externalLienMarkingDAO" class="com.pennanttech.external.silien.dao.ExtLienMarkingDAOImpl">
		<property name="mainDataSource" ref="dataSource" />
		<property name="extDataSource" ref="extDataSource" />
	</bean>

	<bean id="externalHoldMarkingDAO" class="com.pennanttech.external.sihold.dao.ExternalSIHoldMarkingDAOImpl"
		parent="sequenceDao" />

	<bean id="siService" class="com.pennanttech.external.presentment.service.SIService">
		<property name="externalPresentmentDAO" ref="externalPresentmentDAO" />
	</bean>

	<bean id="achService" class="com.pennanttech.external.presentment.service.ACHService">
		<property name="externalPresentmentDAO" ref="externalPresentmentDAO" />
	</bean>

	<bean id="siInternalService" class="com.pennanttech.external.presentment.service.SIInternalService">
		<property name="externalPresentmentDAO" ref="externalPresentmentDAO" />
	</bean>

	<bean id="extPdcService" class="com.pennanttech.external.presentment.service.ExtPDCService">
		<property name="externalPresentmentDAO" ref="externalPresentmentDAO" />
	</bean>

	<bean id="lienFileWritingService" class="com.pennanttech.external.silien.service.LienFileWritingService">
		<property name="externalLienMarkingDAO" ref="externalLienMarkingDAO" />
	</bean>

	<bean id="mandateProcesses" class="com.pennanttech.external.mandate.ExtMandateProcess" parent="abstractInterface" />


	<bean id="extUcicResponseFolderReader" class="com.pennanttech.external.ucic.service.ExtUcicResponseFileReader">
		<property name="extUcicDao" ref="extUcicDao" />
	</bean>

	<bean id="extUcicResponseProcessor" class="com.pennanttech.external.ucic.service.ExtUcicResponseFileProcessor">
		<property name="extUcicDao" ref="extUcicDao" />
		<property name="externalDao" ref="externalDao" />
	</bean>

	<bean id="extUcicResponseAckFileWriter" class="com.pennanttech.external.ucic.service.ExtUcicResponseAckFileWriter">
		<property name="extUcicDao" ref="extUcicDao" />
		<property name="externalDao" ref="externalDao" />
	</bean>


	<!-- <bean id="siHoldFileService" class="com.pennanttech.external.sihold.service.SIHoldFileService"> <property name="externalHoldMarkingDAO" 
		ref="externalHoldMarkingDAO" /> <property name="externalPresentmentDAO" ref="externalPresentmentDAO" /> </bean> -->
	<bean id="applicationContextProvider" class="com.pennanttech.external.app.util.ApplicationContextProvider" />

	<bean id="jobSchedulerExtension" class="com.pennant.pff.jobs.extension.HDFCJobScheduler" />
	<bean id="extMandateExtension" class="com.pennanttech.external.mandate.ExtMandateHandler" />

	<bean id="ucicWeeklyWritingService" class="com.pennanttech.external.ucic.service.ExtUcicWeekFileService">
		<property name="extUcicDao" ref="extUcicDao" />
	</bean>

	<bean id="externalPresentmentHook" class="com.pennanttech.external.presentment.service.PresentmentRequestHandler">
		<property name="externalPresentmentDAO" ref="externalPresentmentDAO" />
		<property name="siService" ref="siService" />
		<property name="achService" ref="achService" />
		<property name="siInternalService" ref="siInternalService" />
		<property name="extPdcService" ref="extPdcService" />
	</bean>
	<bean id="bankAccountValidationService"
		class="com.pennanttech.external.casavalidation.service.CasaAccountValidationService">
		<property name="interfaceLoggingDAO" ref="interfaceLoggingDAO" />
	</bean>


	<bean id="baselOneDumpService" class="com.pennanttech.external.extractions.service.micro.BaselOneDumpService">
		<property name="extExtractionDao" ref="extExtractionDao" />
	</bean>

	<bean id="almDumpService" class="com.pennanttech.external.extractions.service.micro.ALMDumpService">
		<property name="extExtractionDao" ref="extExtractionDao" />
	</bean>

	<bean id="baselTwoDumpService" class="com.pennanttech.external.extractions.service.micro.BaselTwoDumpService">
		<property name="extExtractionDao" ref="extExtractionDao" />
	</bean>

	<bean id="rpmsDumpService" class="com.pennanttech.external.extractions.service.micro.RPMSDumpService">
		<property name="extExtractionDao" ref="extExtractionDao" />
	</bean>

	<bean id="microEodExternalProcessHook"
		class="com.pennanttech.external.extractions.service.micro.ExtExtractionDumpService">
		<property name="baselOneDumpService" ref="baselOneDumpService" />
		<property name="almDumpService" ref="almDumpService" />
		<property name="baselTwoDumpService" ref="baselTwoDumpService" />
		<property name="rpmsDumpService" ref="rpmsDumpService" />
	</bean>

	<bean id="beforeEodExternalProcessHook"
		class="com.pennanttech.external.extractions.service.micro.BeforeEodExternalProcessService">
		<property name="extExtractionDao" ref="extExtractionDao" />
	</bean>

	<bean id="collectionReceiptService"
		class="com.pennanttech.external.collectionreceipt.service.CollectionReceiptService" />
		
	<bean id="extCollectionReceiptDao"
		class="com.pennanttech.external.collectionreceipt.dao.ExtCollectionReceiptDaoImpl">
		<property name="mainDataSource" ref="dataSource" />
		<property name="extDataSource" ref="extDataSource" />
	</bean>
</beans>