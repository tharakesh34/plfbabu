	</changeSet>
	<changeSet id="2" author="surekha">
		<dropColumn columnName="Address" tableName="AdtBankBranches" />
		<dropColumn columnName="Street" tableName="AdtBankBranches" />
		<dropColumn columnName="FlatNo" tableName="AdtBankBranches" />
	</changeSet>
	<changeSet id="3" author="Surekha">
		<addColumn tableName="AdtRMTBranches">
			<column name="BranchAddrHNbr" type="varchar(150)" />
			<column name="BranchFlatNbr" type="varchar(50)" />
			<column name="BranchAddrStreet" type="varchar(50)" />
		</addColumn>
	</changeSet>
	<changeSet id="4" author="Madhubabu">
		<addColumn tableName="ADTfininsurances">
			<column name="PolicyType" type="nvarchar(8)" />
		</addColumn>
	</changeSet>
	<changeSet id="5" author="Madhubabu">
		<dropColumn columnName="PolicyType" tableName="ADTfininsurances" />
	</changeSet>
	<changeSet id="6" author="Madhubabu">
		<addColumn tableName="ADTfininsurances">
			<column name="PolicyCode" type="nvarchar(8)" />
		</addColumn>
	</changeSet>
</databaseChangeLog>
