<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>

<zk xmlns:w="http://www.zkoss.org/2005/zk/client"
	xmlns="http://www.zkoss.org/2005/zul"
	xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

<style>
      .CodeMirror-line-numbers {
        width: 2.2em;
        color: #aaa;
        background-color: #eee;
        text-align: right;
        padding-right: .3em;   
        font-size: 10pt;
        font-family: monospace;
        padding-top: .4em;
        line-height: normal;
      }
    </style>
    
	<script type="text/javascript" src="/ptjs/fulljslint.js" /> 
    		<script type="text/javascript">
			<![CDATA[     
	function addStringContent(strContent) {				
 			zk.Widget.$(jq('$amountRule'))._editor.replaceSelection(strContent);
	}	
	function validateJs(btn) {
		try{	
		var code;	
  		jslint_options = {//json object
							'passfail':false,	// Stop on first error
							'white':false,		// Strict whitespace (enabled in "good parts")
							'browser':false,	// Assume a browser
							'devel':false,		// Assume console, alert,...
							'widget':false,		// Assume Yahoo widget
							'windows':false,	// Assume Windows
							'rhino':false,		// Assume Rhino
							'safe':false,		// Safe Subset
							'adsafe':false,		// ADsafe					 
							'debug':false,		// Tolerate debugger statements
							'evil':false,		// Tolerate eval() statements
							'laxbreak':false,	// Tolerate sloppy linebreaking
							'forin':false,		// Tolerate unfiltered for in
							'sub':false,		// Tolerate inefficient subscripting
							'css':false,		// Tolerate CSS workarounds
							'cap':false,		// Tolerate HTML case
							'on':false,		    // Tolerate HTML event handlers
							'fragment':false,	// Tolerate HTML fragments
							'es5':false,		// Tolerate ES5 syntax					 
							'onevar':false,		// Allow one var statement per function (enabled in "good parts")
							'undef':false,		// Disallow undefined variables (enabled in "good parts")
							'nomen':true,		// Disallow dangling _ in identifiers (enabled in "good parts")
							'eqeqeq':false,		// Disallow == and != (enabled in "good parts")
							'plusplus':true,	// Disallow ++ and -- (enabled in "good parts")
							'bitwise':true,		// Disallow bitwise operators (enabled in "good parts")
							'regexp':true,		// Disallow insecure . and [^...] in /RegExp/ (enabled in "good parts")
							'newcap':true,		// Require Initial Caps for constructors (enabled in "good parts")
							'immed':true,		// Require parens around immediate invocations (enabled in "good parts")
							'strict':false,		// Require "use strict";
							'maxerr':10, 
							'indent':4
						};
  		code=zk.Widget.$(jq('$amountRule'))._editor.getCode();  
  		msgWidget = zk.Widget.$(jq('$btnValidate'));
  		simWidget = zk.Widget.$(jq('$btnSimulate'));
  		savWidget = zk.Widget.$(jq('$btnValidateSave'));
  		
  		if(code != ""){  			
  					JSLINT(code, jslint_options); 			
  					var data=[JSLINT.errors,JSLINT.data().implieds]; 
  					if(btn==1){
  					zAu.send(new zk.Event(msgWidget, 'onUser',data));  					
  					}
  					if(btn==2){
  							zAu.send(new zk.Event(simWidget, 'onUser',data));  		
  					}
  		}			
  					if(btn==3){
  					
  							zAu.send(new zk.Event(savWidget, 'onUser',data));  		
  					}
  		
		}catch(err){
			alert("Error on The Page"+err);
		}
	}
			]]>
			  </script>
			  
	<window>		  
		<window id="window_TransactionEntryDialog" border="normal"
			apply="${transactionEntryDialogCtrl}" width="100%" height="100%"
			sizable="false">
			
			<borderlayout id="borderlayoutTransactionEntry">
				<north>
					<div sclass="z-toolbar" style="padding:0">
						<hbox pack="stretch"
							sclass="hboxRemoveWhiteStrips" width="100%">

							<!-- COMMON BUTTONS -->
							<toolbar align="start"
								sclass="toolbar-start">
								<button id="btnNew" height="24px"
									tooltiptext="${c:l('btnNew.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnEdit" height="24px"
									tooltiptext="${c:l('btnEdit.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnDelete" height="24px"
									tooltiptext="${c:l('btnDelete.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnSave" height="24px"
									tooltiptext="${c:l('btnSave.tooltiptext')}"
									w:onClick="validateJs('3');"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnCancel" height="24px"
									tooltiptext="${c:l('btnCancel.tooltiptext')}" />
								<button id="btnNotes" height="24px"
									tooltiptext="${c:l('btnNotes.tooltiptext')}" visible="false"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnValidate"
									tooltiptext="validate" image="/images/icons/tick_16.png"
									w:onClick="validateJs('1');" />
								<button id="btnSimulate"
									tooltiptext="simulate" image="/images/icons/start.gif"
									w:onClick="validateJs('2');" />
								<button id="btnValidateSave"
									tooltiptext="simulate" image="/images/icons/start.gif"
									visible="false" />
							</toolbar>

							<!-- SEARCH AREA -->
							<toolbar align="center"
								sclass="toolbar-center">
								<label
									value="${c:l('window_TransactionEntryDialog.title')}"
									sclass="label-heading">
								</label>
							</toolbar>

							<!-- CRUD BUTTONS -->
							<toolbar align="end" sclass="toolbar-end">
								<button id="btnHelp" height="24px"
									label="${c:l('label_Help')}"
									tooltiptext="${c:l('btnHelp.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
								<button id="btnClose" height="24px"
									tooltiptext="${c:l('btnClose.tooltiptext')}"
									autodisable="btnNew,btnEdit,btnDelete,btnSave,btnCancel,btnNotes,btnHelp,btnClose" />
							</toolbar>
						</hbox>
					</div>

				</north>
				<center border="none" autoscroll="false">
					<div>
						<groupbox id="gb_basicDetails" mold="3d"
							width="100%">
							<grid id="grid_Basicdetails"
								sclass="GridLayoutNoBorder" fixedLayout="true"
								style="border:0px">
								<columns>
									<column width="200px" />
									<column />
									<column width="180px" />
									<column />
								</columns>
								<rows>
									<row visible="true">
										<label
											id="label_AccountingSetDialog_EventCode"
											value="${c:l('label_AccountingSetDialog_EventCode.value')}" />
										<hbox>
											<space spacing="2px" />
											<textbox id="eventCode"
												width="150px" readonly="true" />
										</hbox>
										<label
											id="label_AccountingSetDialog_EventCodeDesc"
											value="${c:l('label_AccountingSetDialog_EventCodeDesc.value')}" />
										<hbox>
											<space spacing="2px" />
											<textbox
												id="lovDescEventCodeName" width="350px" readonly="true" />
										</hbox>
									</row>
									<row visible="true">
										<label
											id="label_AccountingSetDialog_AccountSetCode"
											value="${c:l('label_AccountingSetDialog_AccountSetCode.value')}" />
										<hbox>
											<space spacing="2px" />
											<textbox id="accountSetCode"
												maxlength="8" width="150px" readonly="true" />
										</hbox>
										<label
											id="label_AccountingSetDialog_AccountSetCodeName"
											value="${c:l('label_AccountingSetDialog_AccountSetCodeName.value')}" />
										<hbox>
											<space spacing="2px" />
											<textbox
												id="accountSetCodeName" maxlength="50" width="350px"
												readonly="true" />
										</hbox>
									</row>
									<row visible="true">
										<label
											id="label_TransactionEntryDialog_TransOrder"
											value="${c:l('label_TransactionEntryDialog_TransOrder.value')}" />
										<hbox>
											<space spacing="2px"
												sclass="mandatory" />
											<intbox id="transOrder"
												width="90px" maxlength="14" />
										</hbox>
										<label
											id="label_TransactionEntryDialog_TransDesc"
											value="${c:l('label_TransactionEntryDialog_TransDesc.value')}" />
										<hbox>
											<space spacing="2px"
												sclass="mandatory" />
											<textbox id="transDesc"
												maxlength="50" width="350px" />
										</hbox>
									</row>
									<row visible="true">
										<label
											id="label_TransactionEntryDialog_Debitcredit"
											value="${c:l('label_TransactionEntryDialog_Debitcredit.value')}" />
										<hbox>
											<space spacing="2px"
												sclass="mandatory" />
											<combobox id="debitcredit"
												width="100px" readonly="true" />
										</hbox>
										<label
											id="label_TransactionEntryDialog_ShadowPosting"
											value="${c:l('label_TransactionEntryDialog_ShadowPosting.value')}" />
										<hbox>
											<space spacing="2px" />
											<checkbox
												id="shadowPosting" />
										</hbox>
									</row>
									<row visible="true">
										<label
											id="label_TransactionEntryDialog_Account"
											value="${c:l('label_TransactionEntryDialog_Account.value')}" />
										<hbox>
											<space spacing="2px"
												sclass="mandatory" />
											<combobox id="account"
												width="225px" readonly="true" />
										</hbox>
										<label
											id="label_TransactionEntryDialog_AccountSubHeadRule"
											value="${c:l('label_TransactionEntryDialog_AccountSubHeadRule.value')}" />
										<hbox>
											<space spacing="2px"
												id="spSubHead" />
											<textbox
												id="accountSubHeadRule" width="72px" visible="false" />
											<textbox
												id="lovDescAccountSubHeadRuleName" width="250px"
												readonly="true" />
											<button label=""
												id="btnSearchAccountSubHeadRule"
												image="/images/icons/LOVSearch.png" height="24px" />
										</hbox>
									</row>
									<row visible="true">
										<label
											id="label_TransactionEntryDialog_AccountType"
											value="${c:l('label_TransactionEntryDialog_AccountType.value')}" />
										<hbox>
											<space spacing="2px"
												id="spAccountType" />
											<textbox id="accountType"
												width="72px" visible="false" />
											<textbox
												id="lovDescAccountTypeName" width="250px" readonly="true" />
											<button label=""
												id="btnSearchAccountType"
												image="/images/icons/LOVSearch.png" height="24px" />
											<button label=""
												id="btnSearchSystemIntAccount"
												image="/images/icons/LOVSearch.png" height="24px"
												visible="false" />
										</hbox>
										<label
											id="label_TransactionEntryDialog_AccountBranch"
											value="${c:l('label_TransactionEntryDialog_AccountBranch.value')}" />
										<hbox>
