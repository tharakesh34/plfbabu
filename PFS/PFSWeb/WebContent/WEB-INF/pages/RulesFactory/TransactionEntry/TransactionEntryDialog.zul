					</div>
				</center>
				<south visible="false">
					<grid>
						<columns>
							<column />
						</columns>
						<rows>
							<row id="statusRow">
								<label
									id="label_TransactionEntryDialog_RecordStatus"
									value="${c:l('label_TransactionEntryDialog_RecordStatus.value')}" />
								<label id="recordStatus" />
							</row>

							<row spans="2">
								<groupbox id="groupboxWf" width="97%">
									<caption label="${c:l('Action')}" />
									<hbox>
										<radiogroup id="userAction"
											name="action" />
									</hbox>
								</groupbox>
							</row>

						</rows>
					</grid>
				</south>

			</borderlayout>
		</window>
	</window>
</zk>
