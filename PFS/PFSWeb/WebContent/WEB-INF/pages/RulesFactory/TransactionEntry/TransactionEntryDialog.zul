											<textbox
												id="lovDescAccountBranchName" width="250px" readonly="true" />
											<button label=""
												id="btnSearchAccountBranch"
												image="/images/icons/LOVSearch.png" height="24px" />
										</hbox>
									</row>
									<row visible="true">
										<label
											id="label_TransactionEntryDialog_TranscationCode"
											value="${c:l('label_TransactionEntryDialog_TranscationCode.value')}" />
										<hbox>
											<space spacing="2px"
												sclass="mandatory" />
											<textbox
												id="transcationCode" width="72px" visible="false" />
											<textbox
												id="lovDescTranscationCodeName" width="250px"
												readonly="true" />
											<button label=""
